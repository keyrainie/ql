           ,@LanguageCode
           ,@CurrencySysNo
           ,@PackStyle
           ,@StoreCompanyCode
           ,@ShipFeeType
           ,@DeliveryType
           ,@DisplayShipName
           ,@DeliveryPromise
           ,@Availsection
           ,@CompensationLimit
           ,@IsSpecified
           ,@IntervalDays
           ,@ContactPhoneNumber
           ,@OfficialWebsite
           ,@Appointment4CombineSO
           ,@StoreType
           ,@MerchantSysNo)
--配送方式扩展信息  
        INSERT INTO
          IPP3.dbo.shiptype_ex
          (
            ShipTypeSysNo,
            AreaSysNo,
            ContactName,
			      Phone,
            Email,
            CompanyCode,
            [Address]
