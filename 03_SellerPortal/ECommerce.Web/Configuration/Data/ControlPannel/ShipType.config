           ,[IsSpecified]
           ,[IntervalDays]
           ,[ContactPhoneNumber]
           ,[OfficialWebsite]
           ,[Appointment4CombineSO]
           ,[StoreType]
           ,[MerchantSysNo])
     VALUES
