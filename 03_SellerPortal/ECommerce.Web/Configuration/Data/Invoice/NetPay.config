<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.ECommerce.com/DataOperation">
  <dataCommand name="GetValidNetPayBySOSysNo" database="Write" commandType="Text">
    <commandText>
      <![CDATA[
SELECT
    [SysNo]
   ,[SOSysNo]
   ,[PayTypeSysNo]
   ,[PayAmount]
   ,[Source]
   ,[InputTime]
   ,[InputUserSysNo]
   ,[ApproveUserSysNo]
   ,[ApproveTime]
   ,[Note]
   ,[Status]
   ,[ReviewedUserSysNo]
   ,[ReviewedTime]
   ,[RelatedSoSysNo]
   ,[ExternalKey]
FROM [OverseaInvoiceReceiptManagement].[dbo].[V_IM_NetPay] WITH(NOLOCK)
WHERE
    [SOSysNo] = @SOSysNo
    AND [Status] <> -1
       ]]>
    </commandText>
    <parameters>
      <param name="@SOSysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
</dataOperations>