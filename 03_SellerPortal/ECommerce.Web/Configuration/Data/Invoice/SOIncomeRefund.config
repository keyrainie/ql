  <dataCommand name="CreateAuditRefund" database="Write" commandType="Text">
    <commandText>
      <![CDATA[
INSERT INTO [IPP3].[dbo].[Finance_SOIncome_BankInfo]
     ([OrderType]
     ,[OrderSysNo]
     ,[SOSysNo]
     ,[BankName]
     ,[BranchBankName]
     ,[CardNumber]
     ,[CardOwnerName]
     ,[PostAddress]
     ,[PostCode]
     ,[ReceiverName]
     ,[RefundPayType]
     ,[CreateUserSysNo]
     ,[CreateTime]
     ,[Status]
     ,[Note]
     ,[HaveAutoRMA]
     ,[RefundCashAmt]
     ,[RefundPoint]
     ,[ToleranceAmt]
     ,[RefundReason]
     ,[CompanyCode]
	 ,[RefundGiftCard]
   ,[CreateUserName]
)
VALUES
     (@OrderType
     ,@OrderSysNo
     ,@SOSysNo
     ,@BankName
     ,@BranchBankName
     ,@CardNumber
     ,@CardOwnerName
     ,@PostAddress
     ,@PostCode
     ,@ReceiverName
     ,@RefundPayType
     ,@CreateUserSysNo
     ,GETDATE()
