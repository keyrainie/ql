     ,@Note
     ,@HaveAutoRMA
     ,@RefundCashAmt
     ,@RefundPoint
     ,@ToleranceAmt
     ,@RefundReason
     ,@CompanyCode
	 ,@RefundGiftCard
   ,@CreateUserName
)
SELECT SCOPE_IDENTITY()
        ]]>
    </commandText>
    <parameters>
      <param name="@OrderType" dbType="Int32"/>
      <param name="@OrderSysNo" dbType="Int32"/>
      <param name="@SOSysNo" dbType="Int32"/>
      <param name="@BankName" dbType="AnsiString" size="50"/>
