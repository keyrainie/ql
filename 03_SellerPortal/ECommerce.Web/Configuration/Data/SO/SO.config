  <dataCommand name="SOPrice_Delete_SOPriceBySOSysNo" database="Write" commandType="Text">
    <commandText>
      <![CDATA[
			        IF EXISTS(SELECT TOP(1) 1 FROM OverseaOrderManagement.dbo.Sub_SO_Master WITH(NOLOCK)
                        WHERE SOSysNo = @SOSysNo )
                    BEGIN
                         DELETE OverseaOrderManagement.dbo.Sub_SO_Item WHERE SOSysNo = @SOSysNo
                         DELETE OverseaOrderManagement.dbo.Sub_SO_Master WHERE SOSysNo = @SOSysNo
                    END
        ]]>
    </commandText>
    <parameters>
      <param name="@SOSysNo" dbType="Int32"/>
    </parameters>
  </dataCommand>

  <dataCommand name="SOPrice_Insert_SOPrice" database="Write" commandType="Text">
    <commandText>
      <![CDATA[				 
INSERT INTO OverseaOrderManagement.dbo.Sub_SO_Master
 (
     SOSysNo,
     WareHouseNumber,
     SoAmt,
     CashPay,
     PremiumAmt,
     ShippingCharge,
     PayPrice,
     PointPay,
     PrepayAmt,
     Promotion,
     Discount,
     InvoiceAmt,
     ReceivableAmt,
     PointAmt,
     InUser,
     InDate,
     Status,
     CompanyCode,
     GiftCardPay
 )
 VALUES (
     @SOSysNo,
     @StockSysNo,
     @SOAmount,
     @CashPay,
     @PremiumAmount,
     @ShipPrice,
     @PayPrice,
     -@PointPayAmount,
     -@PrepayAmount,
     -@CouponAmount,
     @PromotionAmount,
     @InvoiceAmount,
     @ReceivableAmount,
     @GainPoint,
     @InUser,
     GETDATE(),
     @Status,
     @CompanyCode,
     -@GiftCardPay
 )
SELECT SCOPE_IDENTITY()
        ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" direction="Output"/>
