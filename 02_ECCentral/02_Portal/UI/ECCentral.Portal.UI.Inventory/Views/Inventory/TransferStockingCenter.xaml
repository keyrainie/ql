                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="JDPrice" Width="150"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_JDPrice, Source={StaticResource MyResSO}}">
