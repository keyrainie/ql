                                                <ColumnDefinition Width="1"></ColumnDefinition>
                                                <ColumnDefinition Width="80"></ColumnDefinition>
                                                <ColumnDefinition Width="1"></ColumnDefinition>
                                                <ColumnDefinition Width="80"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <ComboBox Name="cmbShiftOutStock_GZ" Grid.Column="0" Tag="{Binding SuggestTransferStocks[2].OutStockShiftQtyDisplay, Mode=TwoWay}"  ItemsSource="{Binding OutStockList_GZ}" SelectedValuePath="Code" DisplayMemberPath="Name"  SelectedValue="{Binding SuggestTransferStocks[2].OutStockWareHouseNumber, Mode=TwoWay}" SelectionChanged="cmbShiftOutStock_GZ_SelectionChanged"></ComboBox>
                                            <Rectangle Fill="#FFF1F1F1" VerticalAlignment="Stretch" Width="1" Grid.Column="1" />
                                            <TextBlock  Grid.Column="2" Text="{Binding SuggestTransferStocks[2].OutStockShiftQtyDisplay, Mode=TwoWay}" HorizontalAlignment="Center"></TextBlock>
                                            <Rectangle Fill="#FFF1F1F1" VerticalAlignment="Stretch" Width="1" Grid.Column="3" />
                                            <TextBox  Grid.Column="4" Text="{Binding SuggestTransferStocks[2].ShiftQty, Mode=TwoWay, NotifyOnValidationError=True}"></TextBox>
                                        </Grid>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                        </ovsDataControls:DataGrid.Columns>
                    </ovsDataControls:DataGrid>
                </toolkit:Expander>
            </Grid>
        </ovsControls:FunctionPanel.Content>
        <ovsControls:FunctionPanel.FunctionContent>
            <!--  Function Fanel区域 Start  -->
            <StackPanel Orientation="Horizontal">
                <Button x:Name="btnTransferStockingAlert"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_TransferStockingAlert, Source={StaticResource MyResSO}}"
                            Click="btnTransferStockingAlert_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/display.png" />
                <Button x:Name="btnPurchase"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_Purchaset, Source={StaticResource MyResSO}}"
                            Click="btnPurchase_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/crdLog.png" />
                <Button x:Name="btnShift"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_Shift, Source={StaticResource MyResSO}}"
                            Click="btnShift_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/cancel-rush-order.png" />
                <Button x:Name="btnViewBasket"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_ViewBasket, Source={StaticResource MyResSO}}"
                            Click="btnViewBasket_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/claim-query.png" />
                <Button x:Name="btnViewShift"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_ViewShift, Source={StaticResource MyResSO}}"
                            Click="btnViewShift_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/claim-query.png" />
                <Button x:Name="btnSync"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_Sync, Source={StaticResource MyResSO}}"
                            Click="btnSync_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/edit.png" />
                <Button x:Name="btnClearPurchase"
                            Margin="10,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="{Binding Path=Button_ClearPurchase, Source={StaticResource MyResSO}}"
                            Click="btnClearPurchase_Click"
                            Style="{StaticResource ButtonBStyle}"
                            Tag="/Themes/Default/Icons/Button/exit.png" />
            </StackPanel>
            <!--  Function Fanel区域 End  -->
        </ovsControls:FunctionPanel.FunctionContent>
    </ovsControls:FunctionPanel>
</ovs:PageBase>
