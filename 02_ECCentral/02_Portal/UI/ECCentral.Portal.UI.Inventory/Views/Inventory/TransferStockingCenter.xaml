<ovs:PageBase x:Class="ECCentral.Portal.UI.Inventory.Views.TransferStockingCenter" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:ovs="clr-namespace:Newegg.Oversea.Silverlight.ControlPanel.Core.Base;assembly=Newegg.Oversea.Silverlight.ControlPanel.Core"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
           xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
           xmlns:ovsControls="clr-namespace:Newegg.Oversea.Silverlight.Controls;assembly=Newegg.Oversea.Silverlight.Controls"
           xmlns:converter="clr-namespace:ECCentral.Portal.Basic.Utilities;assembly=ECCentral.Portal.Basic"
           xmlns:commonConverter="clr-namespace:ECCentral.Portal.Basic.Converters;assembly=ECCentral.Portal.Basic"
           xmlns:ovsDataControls="clr-namespace:Newegg.Oversea.Silverlight.Controls.Data;assembly=Newegg.Oversea.Silverlight.Controls.Data"
           xmlns:ucSearch="clr-namespace:ECCentral.Portal.UI.Inventory.UserControls" 
           xmlns:ucArea="clr-namespace:ECCentral.Portal.Basic.Components.UserControls.AreaPicker;assembly=ECCentral.Portal.Basic"
           xmlns:dataprimitives="clr-namespace:System.Windows.Controls.Primitives;assembly=System.Windows.Controls.Data"
           xmlns:local="clr-namespace:ECCentral.Portal.UI.Inventory.Views.Inventory"
           xmlns:DateConverter="clr-namespace:ECCentral.Portal.Basic.Converters;assembly=ECCentral.Portal.Basic"
           mc:Ignorable="d" d:DesignWidth="640" d:DesignHeight="480"
           xmlns:resSO="clr-namespace:ECCentral.Portal.UI.Inventory.Resources"
           >
    <ovs:PageBase.Resources>
        <Style x:Key="DataGridHeader" TargetType="dataprimitives:DataGridColumnHeader">
            <Setter Property="Foreground" Value="#FFFFFF"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="IsTabStop" Value="False"/>
            <Setter Property="Padding" Value="4"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Grid x:Name="Root">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualState x:Name="Normal"/>
                                    <VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MouseOverState" Storyboard.TargetProperty="Opacity">
                                                <SplineDoubleKeyFrame KeyTime="0" Value="1"/>
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity">
                                                <SplineDoubleKeyFrame KeyTime="0" Value="1"/>
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="SortStates">
                                    <VisualState x:Name="Unsorted"/>
                                    <VisualState x:Name="SortAscending" />
                                    <VisualState x:Name="SortDescending" />
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>

                            <Border x:Name="NormalState"
                                Grid.ColumnSpan="2"
                                BorderBrush="#FF333333" 
                                BorderThickness="0 1">
                                <Grid>
                                    <Rectangle Stroke="#FF333333">
                                        <Rectangle.Effect>
                                            <DropShadowEffect Direction="280" ShadowDepth="1" BlurRadius="0" Opacity="0.05"/>
                                        </Rectangle.Effect>
                                        <Rectangle.Fill>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FF6E6E6E"/>
                                                <GradientStop Color="#FF4D4D4D" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                    <Rectangle>
                                        <Rectangle.Stroke>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FF8C8C8C" Offset="0"/>
                                                <GradientStop Color="#FF5F5F5F" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Stroke>
                                    </Rectangle>
                                </Grid>
                            </Border>

                            <Border x:Name="MouseOverState" 
                                Grid.ColumnSpan="2"  
                                Opacity="0"
                                BorderBrush="#FF333333" 
                                BorderThickness="0 1">
                                <Grid>
                                    <Rectangle Stroke="#FF333333">
                                        <Rectangle.Fill>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FF8A8A8A"/>
                                                <GradientStop Color="#FF6F6F6F" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                    <Rectangle>
                                        <Rectangle.Stroke>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFA5A5A5" Offset="0"/>
                                                <GradientStop Color="#FF7D7D7D" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Stroke>
                                    </Rectangle>
                                </Grid>
                            </Border>

                            <Border x:Name="PressedState"
                                Grid.ColumnSpan="2"
                                Opacity="0"
                                BorderBrush="#FF333333" 
                                BorderThickness="0 1">
                                <Grid>
                                    <Rectangle Stroke="#FF333333">
                                        <Rectangle.Fill>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FF6E6E6E" Offset="1"/>
                                                <GradientStop Color="#FF4D4D4D"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                    <Rectangle>
                                        <Rectangle.Stroke>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FF8C8C8C" Offset="1"/>
                                                <GradientStop Color="#FF5F5F5F"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Stroke>
                                    </Rectangle>
                                </Grid>
                            </Border>
                            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                          			VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0" />
                            </Grid>
                            <Rectangle x:Name="VerticalSeparator" 
									   Fill="#333333" Visibility="Visible" 
                               		   VerticalAlignment="Stretch" Width="1"
                               		   Grid.Row="1" Grid.Column="1"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <DateConverter:DateTimeConverter x:Key="DateTimeConverter" />
        <DateConverter:MoneyConverter x:Key="MoneyConverter"/>
        <resSO:ResTransferStockingCenter x:Name="MyResSO" />
    </ovs:PageBase.Resources>
    <ovsControls:FunctionPanel ContentVerticalScrollBarVisibility="Disabled" ContentHorizontalScrollBarVisibility="Disabled">
        <ovsControls:FunctionPanel.Content>
            <Grid x:Name="LayoutRoot" Background="Transparent" Margin="20 5 20 5">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <!--查询条件-->
                <toolkit:Expander x:Name="SearchCondition"
                                      Grid.Row="0"
                                      Header="查询条件"
                                      IsExpanded="True">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <ucSearch:UCInventoryStockingCenterSearch x:Name="ucSearchCondition" Grid.Row="0"></ucSearch:UCInventoryStockingCenterSearch>
                        <TextBlock Name="lblSearchConditionResult" Foreground="Blue" FontWeight="Bold" Grid.Row="1"  Margin=" 7 10 0 0" TextWrapping="Wrap"></TextBlock>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left"  Grid.Row="2" Margin=" 7 10 0 0">
                            <Button x:Name="btnSearch"
                            Margin="0,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Click="btnSearch_Click"
                            Content="搜索"
                            Tag="/Themes/Default/Icons/Button/query.png" />
                            <Button x:Name="btnReset"
                            Margin="0,1,10,1"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Click="btnReset_Click"
                            Content="重置"
                            Tag="/Themes/Default/Icons/Button/cancel-rush-order.png" />

                        </StackPanel>
                    </Grid>
                </toolkit:Expander>

                <!--查询列表-->
                <toolkit:Expander x:Name="SearchResult"
                                      Grid.Row="1"
                                      Header="{Binding Path=Info_SearchResult, Source={StaticResource MyResSO}}"
                                      IsExpanded="True">
                    <ovsDataControls:DataGrid  Name="GridSearchResult" GridID="C0929ACD-9062-4DA4-8D7A-AF4E4B21ED97" AutoGenerateColumns="False" IsShowColumnsSelector="True" IsShowAllExcelExporter="True" ExportAllClick="GridSearchResult_ExportAllClick"  IsShowPager="True" IsServerPaging="True" IsReadOnly="True" IsServerSort="True"  LoadingDataSource="GridSearchResult_LoadingDataSource" LoadingRow="GridSearchResult_LoadingRow" FrozenColumnCount="3"  EnableCustomizeColumn="True">
                        <ovsDataControls:DataGrid.Columns>
                            <ovsDataControls:DataGridTemplateColumn Width="50" Name="CheckBox" CanUserSort="False">
                                <ovsDataControls:DataGridTemplateColumn.HeaderStyle>
                                    <Style TargetType="dataprimitives:DataGridColumnHeader" BasedOn="{StaticResource FilterColumnHeaderStyle}">
                                        <Setter Property="VerticalContentAlignment" Value="Center"/>
                                        <Setter Property="ContentTemplate">
                                            <Setter.Value>
                                                <DataTemplate>
                                                    <CheckBox Name="ckbSelectAll" Click="ckbSelectAll_Click" />
                                                </DataTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </ovsDataControls:DataGridTemplateColumn.HeaderStyle>
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <CheckBox x:Name="itemChecked" IsChecked="{Binding IsChecked, Mode=TwoWay}"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>

                            <ovsDataControls:DataGridTemplateColumn Name="ItemCode" Width="150"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}" ovsDataControls:DataGridAttached.Header="{Binding Path=LinkButton_ItemCode, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding ItemCode}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtnItemCode" Click="hlbtnItemCode_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="ItemName"  Width="280"  CanUserSort="False" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=LinkButton_ItemName, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding ItemName}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_ItemName" Click="hlbtn_ItemName_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTextColumn Name="ItemSysNumber" Width="70" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_ItemSysNumber, Source={StaticResource MyResSO}}"  Binding="{Binding ItemSysNumber}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="ManufacturerName" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_ManufacturerName, Source={StaticResource MyResSO}}" Binding="{Binding ManufacturerName}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="Brand" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_Brand, Source={StaticResource MyResSO}}" Binding="{Binding Brand}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="AllAvailableQty" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_AllAvailableQty, Source={StaticResource MyResSO}}" Binding="{Binding AllAvailableQty}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="ConsignQty" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_ConsignQty, Source={StaticResource MyResSO}}" Binding="{Binding ConsignQty}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="AllStockAVGDailySales" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=LinkButton_AllStockAVGDailySales, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding AllStockAVGDailySales}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_AllStockAVGDailySales" Click="hlbtn_AllStockAVGDailySales_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="AllStockAvailableSalesDays" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=LinkButton_AllStockAvailableSalesDays, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding AllStockAvailableSalesDays}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_AllStockAvailableSalesDays" Click="hlbtn_AllStockAvailableSalesDays_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTextColumn Name="AllOutStockQuantity" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_AllOutStockQuantity, Source={StaticResource MyResSO}}" Binding="{Binding AllOutStockQuantity}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="VirtualQty" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=LinkButton_VirtualQty, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding VirtualQty}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_VirtualQty" Click="hlbtn_VirtualQty_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTextColumn Name="InventoryQty" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_InventoryQty, Source={StaticResource MyResSO}}" Binding="{Binding InventoryQty}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="PurchasePrice"  Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_PurchasePrice, Source={StaticResource MyResSO}}" Binding="{Binding PurchasePrice}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="OrderQty" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_OrderQty, Source={StaticResource MyResSO}}" Binding="{Binding OrderQty}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="TransferStockQty" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_TransferStockQty, Source={StaticResource MyResSO}}" Binding="{Binding TransferStockQty}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="PurchaseQty" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_PurchaseQty, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding PurchaseQty}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_PurchaseQty" Click="hlbtn_PurchaseQty_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="UnmarketableQty" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_UnmarketableQty, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding UnmarketableQty}" CommandParameter="{Binding ItemSysNumber}" x:Name="hlbtn_UnmarketableQty" Click="hlbtn_UnmarketableQty_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>                            
                            <ovsDataControls:DataGridTextColumn Name="SuggestQtyAllDisplay" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_SuggestQtyAllDisplay, Source={StaticResource MyResSO}}" Binding="{Binding SuggestQtyAllDisplay}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="VirtualPrice" Width="200"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_VirtualPrice, Source={StaticResource MyResSO}}">
                                <ovsDataControls:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <HyperlinkButton Content="{Binding VirtualPrice}" x:Name="hlbtn_VirtualPrice" Click="hlbtn_VirtualPrice_Click"/>
                                    </DataTemplate>
                                </ovsDataControls:DataGridTemplateColumn.CellTemplate>
                            </ovsDataControls:DataGridTemplateColumn>

                            <ovsDataControls:DataGridTextColumn Name="UnitCost" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_UnitCost, Source={StaticResource MyResSO}}" Binding="{Binding UnitCost, Converter={StaticResource MoneyConverter}}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="GrossProfitRate" Width="200" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_GrossProfitRate, Source={StaticResource MyResSO}}" Binding="{Binding GrossProfitRate}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D1" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D1"   Binding="{Binding D1}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D2" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D2"   Binding="{Binding D2}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D3" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D3"   Binding="{Binding D3}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D4" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D4"   Binding="{Binding D4}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D5" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D5"   Binding="{Binding D5}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D6" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D6"   Binding="{Binding D6}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="D7" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="D7"   Binding="{Binding D7}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="W1" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="W1"   Binding="{Binding W1}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="W2" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="W2"   Binding="{Binding W2}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="W3" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="W3"   Binding="{Binding W3}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="W4" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="W4"   Binding="{Binding W4}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="M1" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="M1"   Binding="{Binding M1}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="M2" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="M2"   Binding="{Binding M2}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="M3" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="M3" Binding="{Binding M3}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTextColumn Name="PO_Memo" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_PO_Memo, Source={StaticResource MyResSO}}" Binding="{Binding PO_Memo}"></ovsDataControls:DataGridTextColumn>
                            <ovsDataControls:DataGridTemplateColumn Name="CurrentPrice" Width="150"  CanUserSort="True" HeaderStyle="{StaticResource DataGridHeader}"  ovsDataControls:DataGridAttached.Header="{Binding Path=Info_CurrentPrice, Source={StaticResource MyResSO}}">
