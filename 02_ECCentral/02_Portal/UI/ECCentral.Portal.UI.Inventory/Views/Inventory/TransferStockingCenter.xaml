                            </ovsDataControls:DataGridTemplateColumn>
                            <!--**日本仓 39**-->
                            <ovsDataControls:DataGridTextColumn Name="SH_AvailableQty" Width="150" HeaderStyle="{StaticResource DataGridHeader}" CanUserSort="True" ovsDataControls:DataGridAttached.Header="{Binding Path=Info_SH_AvailableQty, Source={StaticResource MyResSO}}"  Binding="{Binding SuggestTransferStocks[0].AvailableQty}"></ovsDataControls:DataGridTextColumn>
