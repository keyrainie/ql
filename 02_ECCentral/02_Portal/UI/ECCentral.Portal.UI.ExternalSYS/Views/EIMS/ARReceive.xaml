                        <TextBlock VerticalAlignment="Bottom" Grid.Column="6" HorizontalAlignment="Center" Text="{Binding Path=DataGrid_Column_Head_DueIn30,Source={StaticResource EIMSReceivedSource}}" Padding="5,0,0,0" FontWeight="Bold"/>
