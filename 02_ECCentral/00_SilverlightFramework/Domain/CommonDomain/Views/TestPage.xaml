<ovs:PageBase 
    x:Class="Newegg.Oversea.Silverlight.CommonDomain.Views.TestPage" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:ovs="clr-namespace:Newegg.Oversea.Silverlight.ControlPanel.Core.Base;assembly=Newegg.Oversea.Silverlight.ControlPanel.Core"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows" 
    xmlns:ovsControls="clr-namespace:Newegg.Oversea.Silverlight.Controls;assembly=Newegg.Oversea.Silverlight.Controls"
    xmlns:ovsDataControls="clr-namespace:Newegg.Oversea.Silverlight.Controls.Data;assembly=Newegg.Oversea.Silverlight.Controls.Data"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <Grid x:Name="LayoutRoot">
        <ovsDataControls:DataGrid x:Name="dataGrid"
                                  ItemsSource="{Binding}">
            <ovsDataControls:DataGrid.Columns>
                <ovsDataControls:DataGridTextColumn Binding="{Binding UserId}" Header="UserId"></ovsDataControls:DataGridTextColumn>
                <ovsDataControls:DataGridTextColumn Binding="{Binding Pageviews}" Header="Pageviews"></ovsDataControls:DataGridTextColumn>
                <ovsDataControls:DataGridTextColumn Binding="{Binding Url}" Header="Url"></ovsDataControls:DataGridTextColumn>
            </ovsDataControls:DataGrid.Columns>
        </ovsDataControls:DataGrid>
    </Grid>
</ovs:PageBase>
