                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="CheckedState" Storyboard.TargetProperty="(UIElement.Opacity)" Duration="0" To="1"/>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Unchecked"/>
