                                        <GradientStop Color="#FF627684" Offset="1"/>
                                        <GradientStop Color="#FF9FABB6"/>
                                    </LinearGradientBrush>
                                </Ellipse.Stroke>
                            </Ellipse>
                            <Ellipse Height="10" VerticalAlignment="Center" Stroke="#FFBBCCFF" Width="10" HorizontalAlignment="Center">
