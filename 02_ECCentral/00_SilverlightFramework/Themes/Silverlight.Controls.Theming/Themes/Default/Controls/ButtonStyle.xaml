    <Style TargetType="controls:GridSplitter" x:Key="GridSplitterStyle2">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="IsTabStop" Value="true" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:GridSplitter">
                    <Grid x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}">

                        <!-- Horizontal Template -->
                        <Grid x:Name="HorizontalTemplate" 
                              Visibility="Collapsed" 
                              Margin="10,0" 
                              Height="4">
                                <Border BorderBrush="#FFD4D4D4" BorderThickness="1" Background="#FFF1F1F1">
                                    <Rectangle Fill="White" Height="1" VerticalAlignment="Top" Margin="0,0,0,1" StrokeThickness="0"/>
                                </Border>
                                <Canvas HorizontalAlignment="Center" Width="18" Height="2" VerticalAlignment="Center">
                                    <Rectangle Fill="#FFD4D4D4" Height="1" StrokeThickness="0" Width="2"/>
                                    <Rectangle Fill="#FFD4D4D4" Height="1" StrokeThickness="0" Width="2" Canvas.Left="4"/>
                                    <Rectangle Fill="#FFD4D4D4" Height="1" StrokeThickness="0" Width="2" Canvas.Left="8"/>
                                    <Rectangle Fill="#FFD4D4D4" Height="1" StrokeThickness="0" Width="2" Canvas.Left="12"/>
                                    <Rectangle Fill="#FFD4D4D4" Height="1" StrokeThickness="0" Width="2" Canvas.Left="16"/>
                                    <Rectangle Fill="#FFB7B7B7" Height="1" StrokeThickness="0" Width="2" Canvas.Top="1"/>
                                    <Rectangle Fill="#FFB7B7B7" Height="1" StrokeThickness="0" Width="2" Canvas.Left="4" Canvas.Top="1"/>
                                    <Rectangle Fill="#FFB7B7B7" Height="1" StrokeThickness="0" Width="2" Canvas.Left="8" Canvas.Top="1"/>
                                    <Rectangle Fill="#FFB7B7B7" Height="1" StrokeThickness="0" Width="2" Canvas.Left="12" Canvas.Top="1"/>
                                    <Rectangle Fill="#FFB7B7B7" Height="1" StrokeThickness="0" Width="2" Canvas.Left="16" Canvas.Top="1"/>
                                </Canvas>
                        </Grid>

                        <!-- Vertical Template -->
                        <Grid x:Name="VerticalTemplate"
                              Margin="0,10"
                              Width="4">
                            <Border BorderBrush="#FFD4D4D4" BorderThickness="1" Background="#FFF1F1F1">
                                <Rectangle Fill="White" Width="1" VerticalAlignment="Top" Margin="0,0,0,1" StrokeThickness="0"/>
                            </Border>
                            <Canvas HorizontalAlignment="Center" Width="2" Height="18" VerticalAlignment="Center">
                                <Rectangle Fill="#FFD4D4D4" Height="2" StrokeThickness="0" Width="1"/>
                                <Rectangle Fill="#FFD4D4D4" Height="2" StrokeThickness="0" Width="1" Canvas.Top="4"/>
                                <Rectangle Fill="#FFD4D4D4" Height="2" StrokeThickness="0" Width="1" Canvas.Top="8"/>
                                <Rectangle Fill="#FFD4D4D4" Height="2" StrokeThickness="0" Width="1" Canvas.Top="12"/>
                                <Rectangle Fill="#FFD4D4D4" Height="2" StrokeThickness="0" Width="1" Canvas.Top="16"/>
                                <Rectangle Fill="#FFB7B7B7" Height="2" StrokeThickness="0" Width="1" Canvas.Left="1"/>
                                <Rectangle Fill="#FFB7B7B7" Height="2" StrokeThickness="0" Width="1" Canvas.Top="4" Canvas.Left="1"/>
                                <Rectangle Fill="#FFB7B7B7" Height="2" StrokeThickness="0" Width="1" Canvas.Top="8" Canvas.Left="1"/>
                                <Rectangle Fill="#FFB7B7B7" Height="2" StrokeThickness="0" Width="1" Canvas.Top="12" Canvas.Left="1"/>
                                <Rectangle Fill="#FFB7B7B7" Height="2" StrokeThickness="0" Width="1" Canvas.Top="16" Canvas.Left="1"/>
                            </Canvas>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

