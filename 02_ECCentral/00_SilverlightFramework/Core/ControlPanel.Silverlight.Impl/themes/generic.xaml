<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:components="clr-namespace:Newegg.Oversea.Silverlight.Controls.Components;assembly=Newegg.Oversea.Silverlight.ControlPanel.Impl"
                    xmlns:containers="clr-namespace:Newegg.Oversea.Silverlight.Controls.Containers;assembly=Newegg.Oversea.Silverlight.ControlPanel.Impl"
                    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
                    xmlns:controlsPrimitives="clr-namespace:Newegg.Oversea.Silverlight.Controls.Containers;assembly=Newegg.Oversea.Silverlight.ControlPanel.Impl"
                    xmlns:localResource="clr-namespace:Newegg.Oversea.Silverlight.ControlPanel.Impl.Resources"
                    xmlns:ovscontrols="clr-namespace:Newegg.Oversea.Silverlight.Controls;assembly=Newegg.Oversea.Silverlight.Controls"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows">

    <!--  1.String Resources  -->
    <sys:String x:Key="BrowserDefaultFontSize">12</sys:String>
    <sys:String x:Key="BrowserDefaultFontFamily">Tahoma,SimSun,PMingLiU</sys:String>
    <localResource:MessageResource x:Key="LocalResource" />

    <!--  PageBrowser  -->
    <Style TargetType="containers:PageBrowser">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="containers:PageBrowser">
                    <Grid>
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualStateGroup.Transitions>
                                    <vsm:VisualTransition GeneratedDuration="0" />
                                </vsm:VisualStateGroup.Transitions>
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="Disabled" />
                                <vsm:VisualState x:Name="SinglePage">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="panelNavigation" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TabsViewer" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="cmbTabs" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="MultiPage">
                                    <Storyboard>
                                        <!--
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="panelNavigation" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                            <DiscreteObjectKeyFrame.Value>
                                            <Visibility>Collapsed</Visibility>
                                            </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                            </ObjectAnimationUsingKeyFrames>
                                        -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TabsViewer" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="cmbTabs" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        <!--  TabStripPlacement Top  -->
                        <Grid x:Name="TemplateTop" Visibility="Collapsed">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28" />
                                <RowDefinition Height="26" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <!--  Tab header  -->
                            <Grid Grid.Row="0"
                                  Grid.Column="0"
                                  Margin="62,0,0,0"
                                  Canvas.ZIndex="10">
                                <Grid.Resources>
                                    <Style x:Key="btnTabsStyle" TargetType="Button">
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="Button">
                                                    <Grid Width="{TemplateBinding Width}"
                                                          Height="{TemplateBinding Height}"
                                                          Margin="{TemplateBinding Margin}"
                                                          HorizontalAlignment="Center"
                                                          VerticalAlignment="Center">
                                                        <vsm:VisualStateManager.VisualStateGroups>
                                                            <vsm:VisualStateGroup x:Name="CommonStates">
                                                                <vsm:VisualState x:Name="Normal" />
                                                                <vsm:VisualState x:Name="MouseOver">
                                                                    <Storyboard>
                                                                        <ObjectAnimationUsingKeyFrames Duration="0"
                                                                                                       Storyboard.TargetName="imgSelectTab"
                                                                                                       Storyboard.TargetProperty="Source">
                                                                            <DiscreteObjectKeyFrame KeyTime="0" Value="/Themes/Default/Images/PageBrower/down2_icon.png" />
                                                                        </ObjectAnimationUsingKeyFrames>
                                                                    </Storyboard>
                                                                </vsm:VisualState>
                                                            </vsm:VisualStateGroup>
                                                        </vsm:VisualStateManager.VisualStateGroups>
                                                        <Grid Width="11" Height="11">
                                                            <Image x:Name="imgSelectTab" Source="/Themes/Default/Images/PageBrower/down1_icon.png" />
                                                        </Grid>
                                                    </Grid>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                    <Style x:Key="btnLeftScrollerStyle" TargetType="RepeatButton">
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="RepeatButton">
                                                    <Grid Width="{TemplateBinding Width}"
                                                          Height="{TemplateBinding Height}"
                                                          Margin="{TemplateBinding Margin}"
                                                          HorizontalAlignment="Center"
                                                          VerticalAlignment="Center">
                                                        <vsm:VisualStateManager.VisualStateGroups>
                                                            <vsm:VisualStateGroup x:Name="CommonStates">
                                                                <vsm:VisualState x:Name="Normal" />
                                                                <vsm:VisualState x:Name="MouseOver">
                                                                    <Storyboard>
                                                                        <ObjectAnimationUsingKeyFrames Duration="0"
                                                                                                       Storyboard.TargetName="imgLeftScroller"
                                                                                                       Storyboard.TargetProperty="Source">
                                                                            <DiscreteObjectKeyFrame KeyTime="0" Value="/Themes/Default/Images/PageBrower/LeftHoverScroller.png" />
                                                                        </ObjectAnimationUsingKeyFrames>
                                                                    </Storyboard>
                                                                </vsm:VisualState>
                                                            </vsm:VisualStateGroup>
                                                        </vsm:VisualStateManager.VisualStateGroups>
                                                        <Image x:Name="imgLeftScroller" Source="/Themes/Default/Images/PageBrower/LeftScroller.png" />
                                                    </Grid>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                    <Style x:Key="btnRightScrollerStyle" TargetType="RepeatButton">
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="RepeatButton">
                                                    <Grid Width="{TemplateBinding Width}"
                                                          Height="{TemplateBinding Height}"
                                                          Margin="{TemplateBinding Margin}"
                                                          HorizontalAlignment="Center"
                                                          VerticalAlignment="Center">
                                                        <vsm:VisualStateManager.VisualStateGroups>
                                                            <vsm:VisualStateGroup x:Name="CommonStates">
                                                                <vsm:VisualState x:Name="Normal" />
                                                                <vsm:VisualState x:Name="MouseOver">
                                                                    <Storyboard>
                                                                        <ObjectAnimationUsingKeyFrames Duration="0"
                                                                                                       Storyboard.TargetName="imgRightScroller"
                                                                                                       Storyboard.TargetProperty="Source">
                                                                            <DiscreteObjectKeyFrame KeyTime="0" Value="/Themes/Default/Images/PageBrower/RightHoverScroller.png" />
                                                                        </ObjectAnimationUsingKeyFrames>
                                                                    </Storyboard>
                                                                </vsm:VisualState>
                                                            </vsm:VisualStateGroup>
                                                        </vsm:VisualStateManager.VisualStateGroups>
                                                        <Image x:Name="imgRightScroller" Source="/Themes/Default/Images/PageBrower/RightScroller.png" />
                                                    </Grid>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                    <Style x:Key="cmbTabsStyle" TargetType="ComboBox">
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="ComboBox">
                                                    <Grid>
                                                        <Grid.Resources>
                                                            <Style x:Key="comboToggleStyle" TargetType="ToggleButton">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="ToggleButton">
                                                                            <Grid>
                                                                                <vsm:VisualStateManager.VisualStateGroups>
                                                                                    <vsm:VisualStateGroup x:Name="CommonStates">
                                                                                        <vsm:VisualState x:Name="Normal" />
                                                                                        <vsm:VisualState x:Name="MouseOver" />
                                                                                        <vsm:VisualState x:Name="Pressed" />
                                                                                        <vsm:VisualState x:Name="Disabled" />
                                                                                    </vsm:VisualStateGroup>
                                                                                    <vsm:VisualStateGroup x:Name="CheckStates">
                                                                                        <vsm:VisualState x:Name="Checked">
                                                                                            <Storyboard>
                                                                                                <ObjectAnimationUsingKeyFrames Duration="0"
                                                                                                                               Storyboard.TargetName="PathChecked"
                                                                                                                               Storyboard.TargetProperty="Visibility">
                                                                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                                                                                </ObjectAnimationUsingKeyFrames>
                                                                                            </Storyboard>
                                                                                        </vsm:VisualState>
                                                                                        <vsm:VisualState x:Name="Unchecked" />
                                                                                    </vsm:VisualStateGroup>
                                                                                    <vsm:VisualStateGroup x:Name="FocusStates">
                                                                                        <vsm:VisualState x:Name="Focused" />
                                                                                        <vsm:VisualState x:Name="Unfocused" />
                                                                                    </vsm:VisualStateGroup>
                                                                                </vsm:VisualStateManager.VisualStateGroups>

                                                                                <Grid Margin="0">
                                                                                    <Path Width="28"
                                                                                          Height="24"
                                                                                          Data="M24.5,0.5 L27.5,0.5 L27.5,2.5000005 L27.5,3.5000007 L27.5,4.5 L27.5,20.5 L27.5,23.5 L24.5,23.5 L4.5,23.5 L3.5,23.5 L0.5,23.5 L0.5,20.5 L0.5,19.5 L0.5,3.5000007 C0.5,1.8431463 1.8431454,0.50000066 3.5,0.50000083 z"
                                                                                          Stretch="Fill"
                                                                                          UseLayoutRounding="False">
                                                                                        <Path.Fill>
                                                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                                                <LinearGradientBrush.RelativeTransform>
                                                                                                    <CompositeTransform CenterX="0.5"
                                                                                                                        CenterY="0.5"
                                                                                                                        Rotation="180" />
                                                                                                </LinearGradientBrush.RelativeTransform>
                                                                                                <GradientStop Color="#FF454545" />
                                                                                                <GradientStop Offset="0.97" Color="#FF5C5C5C" />
                                                                                                <GradientStop Offset="1" Color="White" />
                                                                                            </LinearGradientBrush>
                                                                                        </Path.Fill>
                                                                                        <Path.Stroke>
                                                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                                                <GradientStop Offset="0" Color="#FF373737" />
                                                                                                <GradientStop Offset="1" Color="#FF202020" />
                                                                                            </LinearGradientBrush>
                                                                                        </Path.Stroke>
                                                                                    </Path>

                                                                                    <Path x:Name="PathChecked"
                                                                                          Data="M24.5,0.5 L27.5,0.5 L27.5,2.5000005 L27.5,3.5000007 L27.5,4.5 L27.5,20.5 L27.5,23.5 L24.5,23.5 L4.5,23.5 L3.5,23.5 L0.5,23.5 L0.5,20.5 L0.5,19.5 L0.5,3.5000007 C0.5,1.8431463 1.8431454,0.50000066 3.5,0.50000083 z"
                                                                                          Stretch="Fill"
                                                                                          UseLayoutRounding="False"
                                                                                          Visibility="Collapsed">
                                                                                        <Path.Fill>
                                                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                                                <LinearGradientBrush.RelativeTransform>
                                                                                                    <CompositeTransform CenterX="0.5"
                                                                                                                        CenterY="0.5"
                                                                                                                        Rotation="180" />
                                                                                                </LinearGradientBrush.RelativeTransform>
                                                                                                <GradientStop Color="#FF999999" />
                                                                                                <GradientStop Offset="0.97" Color="#FF5C5C5C" />
                                                                                                <GradientStop Offset="1" Color="White" />
                                                                                            </LinearGradientBrush>
                                                                                        </Path.Fill>
                                                                                        <Path.Stroke>
                                                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                                                <GradientStop Offset="0" Color="#FF373737" />
                                                                                                <GradientStop Offset="1" Color="#FF202020" />
                                                                                            </LinearGradientBrush>
                                                                                        </Path.Stroke>
                                                                                    </Path>


                                                                                    <Canvas Width="7"
                                                                                            Height="4"
                                                                                            Margin="0"
                                                                                            HorizontalAlignment="Center"
                                                                                            VerticalAlignment="Center">
                                                                                        <Path Width="7"
                                                                                              Height="4"
                                                                                              Data="M2,2 L5,2 L5,3 L4,3 L4,3.9999995 L3,3.9999995 L3,3 L2,3 z M0,0 L7,0 L7,1 L6,1 L6,2 L1.0000001,2 L1.0000001,1 L0,1 z"
                                                                                              Fill="White"
                                                                                              Stretch="Fill"
                                                                                              UseLayoutRounding="False">
                                                                                            <Path.Effect>
                                                                                                <DropShadowEffect BlurRadius="3" ShadowDepth="1" />
                                                                                            </Path.Effect>
                                                                                        </Path>
                                                                                    </Canvas>
                                                                                </Grid>
                                                                            </Grid>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Grid.Resources>
                                                        <vsm:VisualStateManager.VisualStateGroups>
                                                            <vsm:VisualStateGroup x:Name="CommonStates">
                                                                <vsm:VisualState x:Name="Normal" />
                                                                <vsm:VisualState x:Name="MouseOver" />
                                                                <vsm:VisualState x:Name="Disabled" />
                                                            </vsm:VisualStateGroup>
                                                            <vsm:VisualStateGroup x:Name="FocusStates">
                                                                <vsm:VisualState x:Name="Focused" />
                                                                <vsm:VisualState x:Name="Unfocused" />
                                                                <vsm:VisualState x:Name="FocusedDropDown">
                                                                    <Storyboard>
                                                                        <ObjectAnimationUsingKeyFrames Duration="00:00:00"
                                                                                                       Storyboard.TargetName="PopupBorder"
                                                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                                                            <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                                                                <DiscreteObjectKeyFrame.Value>
                                                                                    <Visibility>Visible</Visibility>
                                                                                </DiscreteObjectKeyFrame.Value>
                                                                            </DiscreteObjectKeyFrame>
                                                                        </ObjectAnimationUsingKeyFrames>
                                                                    </Storyboard>
                                                                </vsm:VisualState>
                                                            </vsm:VisualStateGroup>
                                                            <vsm:VisualStateGroup x:Name="ValidationStates">
                                                                <vsm:VisualState x:Name="Valid" />
                                                                <vsm:VisualState x:Name="InvalidUnfocused" />
                                                                <vsm:VisualState x:Name="InvalidFocused" />
                                                            </vsm:VisualStateGroup>
                                                        </vsm:VisualStateManager.VisualStateGroups>
                                                        <Border x:Name="ContentPresenterBorder">
                                                            <Grid>
                                                                <ToggleButton x:Name="DropDownToggle"
                                                                              HorizontalAlignment="Stretch"
                                                                              VerticalAlignment="Stretch"
                                                                              HorizontalContentAlignment="Center"
                                                                              Style="{StaticResource comboToggleStyle}" />
                                                                <ContentPresenter x:Name="ContentPresenter"
                                                                                  Margin="{TemplateBinding Padding}"
                                                                                  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                                                  Opacity="0">
                                                                    <TextBlock Visibility="Collapsed" />
                                                                </ContentPresenter>
                                                            </Grid>
                                                        </Border>


                                                        <Popup x:Name="Popup">
                                                            <Grid x:Name="PopupBorder"
                                                                  Width="160"
                                                                  HorizontalAlignment="Left"
                                                                  VerticalAlignment="Top">
                                                                <Border Margin="0,-1,0,0"
                                                                        Background="#FF999999"
                                                                        CornerRadius="0,0,2,2">
                                                                    <Border.Effect>
                                                                        <DropShadowEffect Direction="280"
                                                                                          Opacity="0.5"
                                                                                          ShadowDepth="0" />
                                                                    </Border.Effect>
                                                                </Border>
                                                                <ScrollViewer x:Name="ScrollViewer"
                                                                              Margin="0,10"
                                                                              BorderThickness="0"
                                                                              Padding="0">
                                                                    <ItemsPresenter />
                                                                </ScrollViewer>
                                                                <Image Height="3"
                                                                       Margin="0,-2,0,0"
                                                                       VerticalAlignment="Top"
                                                                       Opacity="0.5"
                                                                       Source="/Themes/Default/Images/Shadow/dropMenu.png"
                                                                       Stretch="None" />
                                                                <Image Height="3"
                                                                       Margin="0,0,0,-2"
                                                                       VerticalAlignment="Bottom"
                                                                       Source="/Themes/Default/Images/Shadow/dropMenu.png"
                                                                       Stretch="Fill" />
                                                            </Grid>




                                                            <!--
                                                                <Grid x:Name="PopupBorder"
                                                                HorizontalAlignment="Left"
                                                                VerticalAlignment="Top">
                                                                <Border Background="#FF999999"
                                                                CornerRadius="0,0,2,2"
                                                                Margin="1,0,0,2">
                                                                <Border.Effect>
                                                                <DropShadowEffect Direction="280" ShadowDepth="0" Opacity="0.5"/>
                                                                </Border.Effect>
                                                                </Border>
                                                                <ScrollViewer x:Name="ScrollViewer" MinWidth="150"
                                                                BorderThickness="0"
                                                                Padding="1"
                                                                Margin="0,10">
                                                                <ItemsPresenter/>
                                                                </ScrollViewer>
                                                                </Grid>
                                                            -->
                                                        </Popup>
                                                    </Grid>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </Grid.Resources>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <!--  Tab Header Begin  -->
                                <ScrollViewer x:Name="TabsViewer"
                                              Grid.Column="0"
                                              Margin="0,0,0,-5"
                                              BorderThickness="0"
                                              HorizontalScrollBarVisibility="Disabled"
                                              Padding="5,0,5,0"
                                              VerticalScrollBarVisibility="Disabled">
                                    <ScrollViewer.Content>
                                        <controlsPrimitives:PageBrowserTabPanel x:Name="TabPanelTop"
                                                                                Margin="2,2,2,0"
                                                                                VerticalAlignment="Bottom"
                                                                                Canvas.ZIndex="1" />
                                    </ScrollViewer.Content>
                                </ScrollViewer>
                                <!--  Tab Header End  -->
                                <!--  ToolBar Begin  -->
                                <StackPanel x:Name="ToolBar"
                                            Grid.Column="1"
                                            Margin="10,0,0,0"
                                            Orientation="Horizontal">
                                    <StackPanel.Resources>
                                        <Style x:Key="btnBar" TargetType="Button">
                                            <Setter Property="Cursor" Value="Hand" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="Button">
                                                        <Grid>
                                                            <Rectangle x:Name="btnToolBar"
                                                                       Width="16"
                                                                       Height="16"
                                                                       Margin="0,0,10,1"
                                                                       Fill="{TemplateBinding Background}" />
                                                        </Grid>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </StackPanel.Resources>
                                </StackPanel>
                                <!--  ToolBar End  -->
                                <!--  TabsPopList Begin  -->
                                <StackPanel Grid.Column="2"
                                            Margin="0 0 0 -1"
                                            Orientation="Horizontal">
                                    <ComboBox x:Name="cmbTabs"
                                              Width="28"
                                              Height="24"
                                              Margin="0,0,-1,0"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Bottom"
                                              HorizontalContentAlignment="Stretch"
                                              VerticalContentAlignment="Stretch"
                                              ItemContainerStyle="{StaticResource cmbItemBStyle}"
                                              Style="{StaticResource cmbTabsStyle}">
                                        <ComboBoxItem HorizontalAlignment="Stretch"
                                                      HorizontalContentAlignment="Stretch"
                                                      Style="{StaticResource cmbItemBStyle}">
                                            <ComboBoxItem.Content>
                                                <Grid>
                                                    <TextBlock FontFamily="{StaticResource BrowserDefaultFontFamily}"
                                                               FontSize="12"
                                                               Text="{Binding Source={StaticResource LocalResource},
                                                                              Path=PageBrowser_CloseAllTabs_Title}" />
                                                    <Border BorderBrush="#ACA899" BorderThickness="0,0,0,1" />
                                                </Grid>
                                            </ComboBoxItem.Content>
                                        </ComboBoxItem>
                                    </ComboBox>

                                    <!--  <components:TabList VerticalAlignment="Bottom" />  -->

                                </StackPanel>
                                <!--  TabsPopList End  -->
                            </Grid>
                            <!--  Breadcrumb  -->
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Rectangle Grid.ColumnSpan="2"
                                           Margin="-1,0"
                                           Stroke="#FFff6400">
                                    <Rectangle.Effect>
                                        <DropShadowEffect BlurRadius="2"
                                                          Direction="290"
                                                          Opacity="0.3"
                                                          ShadowDepth="1" />
                                    </Rectangle.Effect>
                                    <Rectangle.Fill>
                                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                            <LinearGradientBrush.RelativeTransform>
                                                <CompositeTransform CenterX="0.5"
                                                                    CenterY="0.5"
                                                                    Rotation="180" />
                                            </LinearGradientBrush.RelativeTransform>
                                            <GradientStop Offset="0.16" Color="#FFff6300" />
                                            <GradientStop Offset="0.8" Color="#FFff9d42" />
                                            <GradientStop Offset="0.51" Color="#FFff8527" />
                                            <GradientStop Offset="0.97" Color="#FFff9d42" />
                                            <GradientStop Offset="1" Color="#FF7A8FAF" />
                                        </LinearGradientBrush>
                                    </Rectangle.Fill>
                                </Rectangle>



                                <StackPanel x:Name="panelNavigation"
                                            Margin="10,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <StackPanel.Resources>
                                        <SolidColorBrush x:Key="brushURLLink" Color="#ffffff" />
                                        <SolidColorBrush x:Key="brushPageTitle" Color="#ffffff" />
                                    </StackPanel.Resources>
                                </StackPanel>
                                <ContentPresenter x:Name="contentPageDescription"
                                                  Grid.Column="1"
                                                  Margin="0 0 10 0"
                                                  HorizontalAlignment="Right"
                                                  VerticalAlignment="Center" />
                            </Grid>
                            <Grid Grid.Row="2"
                                  Grid.Column="0"
                                  Canvas.ZIndex="1">
                                <!--  Tab Content Begin  -->

                                <ScrollViewer x:Name="ContentViewer"
                                              Margin="0"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Stretch"
                                              Background="Transparent"
                                              BorderThickness="0"
                                              HorizontalScrollBarVisibility="Auto"
                                              Padding="0"
                                              VerticalScrollBarVisibility="Auto">
                                    <Grid>
                                        <ovscontrols:DropTarget x:Name="ContentDropTarget" ShowHover="False">
                                            <ovscontrols:DropTarget.Content>
                                                <ovscontrols:DragSource DraggingEnabled="False">
                                                    <Grid Margin="0"
                                                          HorizontalAlignment="Stretch"
                                                          VerticalAlignment="Stretch" />
                                                </ovscontrols:DragSource>
                                            </ovscontrols:DropTarget.Content>
                                        </ovscontrols:DropTarget>


                                        <ContentPresenter x:Name="ContentTop"
                                                          Margin="0"
                                                          HorizontalAlignment="Stretch"
                                                          VerticalAlignment="Stretch" />
                                    </Grid>
                                </ScrollViewer>

                                <!--  Tab Content End  -->
                            </Grid>
                            <!--  MessageBox Begin  -->

                            <StackPanel x:Name="stackMessageBox"
                                        Grid.Row="2"
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Top"
                                        Canvas.ZIndex="99999"
                                        Visibility="Collapsed">
                                <StackPanel.Resources>
                                    <SolidColorBrush x:Key="borderTitle_boderColor_Success" Color="Transparent" />
                                    <SolidColorBrush x:Key="borderTitle_boderColor_Information" Color="Transparent" />
                                    <SolidColorBrush x:Key="borderTitle_boderColor_Warning" Color="Transparent" />
                                    <SolidColorBrush x:Key="borderTitle_boderColor_Error" Color="Transparent" />

                                    <LinearGradientBrush x:Key="borderTitle_Success" StartPoint="0.5,0" EndPoint="0.5,1">
                                        <GradientStop Offset="0" Color="#FF58B449" />
