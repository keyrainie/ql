      ,[PayTypeSysNo]
      ,[PayAmount]
      ,[Source]
      ,[InputTime]
      ,[InputUserSysNo]
      ,[ApproveUserSysNo]
      ,[ApproveTime]
      ,[Note]
      ,[Status]
      ,[ReviewedUserSysNo]
      ,[ReviewedTime]
      ,[RelatedSoSysNo]
