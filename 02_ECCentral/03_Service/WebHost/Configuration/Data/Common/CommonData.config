  <dataCommand name="QueryConfigurationByKey" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT [value] from  IPP3.dbo.Sys_Configuration with (nolock) where [key] = '@Key' AND CompanyCode=@CompanyCode
