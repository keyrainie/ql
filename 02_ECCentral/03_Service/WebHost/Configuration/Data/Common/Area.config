<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation">
  <dataCommand name="GetAreaList" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
SELECT @TotalCount = COUNT(1) FROM [IPP3].[dbo].[Area] WITH(NOLOCK) #StrWhere#

SELECT [SysNo]
      ,[ProvinceSysNo]
      ,[CitySysNo]
      ,[ProvinceName]
      ,[CityName]
      ,[DistrictName]
      ,[OrderNumber]
      ,Convert(bit,IsLocal) AS [IsLocal]
      ,[FullName]
      ,[AreaName]
      ,[AreaType]
      ,[Status]
      ,[Rank]
      ,[SOAmountLimit]
      ,[WeightLimit]
      ,[CompanyCode]
FROM (SELECT TOP (@EndNumber) [SysNo]
          ,[ProvinceSysNo]
          ,[CitySysNo]
          ,[ProvinceName]
          ,[CityName]
          ,[DistrictName]
          ,[OrderNumber]
          ,[IsLocal]
          ,[ProvinceName]+IsNull(','+[CityName],'')+IsNull(','+[DistrictName],'') AS [FullName]
          ,  CASE WHEN ([CityName] IS Null AND [DistrictName] IS NUll)
                  THEN [ProvinceName]
                  WHEN ([CityName] IS NOT Null AND [DistrictName] IS  NUll)
                  THEN [CityName] 
                  WHEN ([CityName] IS NOT Null AND [DistrictName] IS NOT  NUll)
                  THEN [DistrictName] 
                  END AS [AreaName]
          ,  CASE WHEN ([CityName] IS Null AND [DistrictName] IS NUll)
                  THEN  '省'
                  WHEN ([CityName] IS NOT Null AND [DistrictName] IS  NUll)
                  THEN  '市' 
                  WHEN ([CityName] IS NOT Null AND [DistrictName] IS NOT  NUll)
                  THEN '区县' 
                  END AS [AreaType]
          ,[Status]
          ,[Rank]
          ,[SOAmountLimit]
          ,[WeightLimit]
          ,[CompanyCode]
          ,ROW_NUMBER() OVER(ORDER BY #SortColumnName#) AS RowNumber
      FROM [IPP3].[dbo].[Area] WITH(NOLOCK)
      #StrWhere#
) Result
WHERE RowNumber > @StartNumber
       ]]>
    </commandText>
  </dataCommand>

  <dataCommand name="GetArea" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
SELECT [SysNo]
      ,[ProvinceSysNo]
      ,[CitySysNo]
      ,[ProvinceName]
      ,[CityName]
      ,[ProvinceName]+IsNull(','+[CityName],'') AS [FullName]
      ,        CASE WHEN ([CityName] IS Null)
                    THEN [ProvinceName]
                    WHEN ([CityName] IS NOT Null)
                    THEN [CityName] 
                    END AS [AreaName]
      ,[OrderNumber]
      ,[IsLocal]
      ,[Status]
      ,[Rank]
      ,[SOAmountLimit]
      ,[WeightLimit]
      ,[CompanyCode]
FROM [IPP3].[dbo].[Area] WITH(NOLOCK)
WHERE [SysNo]=@SysNo
        ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32"/>
    </parameters>
  </dataCommand>

