<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation"
				xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <dataCommand name="QCSubjectQuery" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[			
  SELECT @TotalCount = COUNT(1)
            FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject] WITH(NOLOCK)                                 
              #StrWhere#
           
             SELECT      [Sysno]
                        ,[Name]
                        ,[OrderNum]
                        ,[ParentSysNo]
                        ,[CreateDate]
                        ,[CreateUserSysNo]
                        ,[LastEditDate]
                        ,[LastEditUserSysNo]
                        ,[CompanyCode]
                        ,[LanguageCode]
                        ,[StoreCompanyCode]
                        ,[Status]                                             
                FROM(
                  SELECT 
                    	   [Sysno]
                        ,[Name]
                        ,[OrderNum]
                        ,[ParentSysno]
                        ,[CreateDate]
                        ,[CreateUserSysNo]
                        ,[LastEditDate]
                        ,[LastEditUserSysNo]
                        ,[CompanyCode]
                        ,[LanguageCode]
                        ,[StoreCompanyCode]
                        ,[Status]                          
                        ,ROW_NUMBER() OVER(ORDER BY #SortColumnName#) AS RowNumber
                  FROM  [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject] WITH(NOLOCK)                                              
                  #StrWhere#) Result
               WHERE RowNumber > @StartNumber and RowNumber <= @EndNumber
      ]]>
    </commandText>
  </dataCommand>

  <dataCommand name="CreateQCSubject" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[	
      INSERT INTO [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject]
           ([Name]
           ,[OrderNum]
           ,[ParentSysNo]
           ,[CreateDate]
           ,[CreateUserSysNo]
           ,[CompanyCode]
           ,[StoreCompanyCode]
           ,[Status])
     VALUES
           (@Name
           ,@OrderNum
           ,@ParentSysno
           ,getdate()
           ,@CreateUserSysNo
           ,@CompanyCode
           ,@CompanyCode
           ,@Status
           )
           ]]>
    </commandText>
    <parameters>
      <param name="@Name" dbType="String"/>
      <param name="@OrderNum" dbType="Int32" />
      <param name="@ParentSysNo" dbType="Int32"  />
      <param name="@Status" dbType="Int32"  />
      <param name="@CreateUserSysNo" dbType="Int32" property="[UserSysNo]"/>
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50"/>
      <!--<param name="@ChannelID" dbType="AnsiStringFixedLength" size="50"/>-->
    </parameters>
  </dataCommand>

  <dataCommand name="UpdateQCSubject" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[	
      UPDATE [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject]
       SET [Name] = @Name
          ,[OrderNum] = @OrderNum
          ,[ParentSysno] = @ParentSysno
          ,[LastEditDate] = getdate()
          ,[LastEditUserSysNo] =@LastEditUserSysNo
          ,[CompanyCode] = @CompanyCode
          ,[StoreCompanyCode] = @CompanyCode
          ,[Status] =@Status
     WHERE  SysNo=@SysNo
            ]]>
    </commandText>
    <parameters>
      <param name="@Name" dbType="String" />
      <param name="@OrderNum" dbType="Int32" />
      <param name="@ParentSysNo" dbType="Int32"  />
      <param name="@Status" dbType="Int32"  />
      <param name="@LastEditUserSysNo" dbType="Int32" property="[UserSysNo]"/>
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50"/>
      <param name="@SysNo" dbType="Int32"  />
    </parameters>
  </dataCommand>

  <dataCommand name="GetParentsQCSubject" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[	
      
SELECT 
    [Sysno] 
   ,[Name] 
   ,[OrderNum] 
   ,[ParentSysNo] 
   ,[CreateDate] 
   ,[CreateUserSysNo] 
   ,[LastEditDate] 
   ,[LastEditUserSysNo] 
   ,[CompanyCode] 
   ,[LanguageCode] 
   ,[StoreCompanyCode] 
   ,[Status] 
FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject] WITH(NOLOCK) 
WHERE 
    ParentSysno=( 
            SELECT 
                ParentSysno 
            FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject] WITH(NOLOCK) 
            WHERE 
                Sysno=( 
                        SELECT 
                            ParentSysno 
                        FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject]  WITH(NOLOCK) 
                        WHERE 
                            Sysno=@SysNo
                    )
        )
 
           ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32"  />
    </parameters>
  </dataCommand>

  <dataCommand name="GetChildrenQCSubject" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[	
        SELECT [Sysno]
      ,[Name]
      ,[OrderNum]
      ,[ParentSysNo]
      ,[CreateDate]
      ,[CreateUserSysNo]
      ,[LastEditDate]
      ,[LastEditUserSysNo]
      ,[CompanyCode]
      ,[LanguageCode]
      ,[StoreCompanyCode]
      ,[Status]
  FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_QSubject]  WITH(NOLOCK) 
  WHERE ParentSysno=@SysNo ORDER BY ordernum,LastEditDate DESC 
           ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32"  />
    </parameters>
  </dataCommand>
</dataOperations>