          ,InventoryType
        FROM IPP3.dbo.Product WITH(NOLOCK)
        WHERE SysNo = @ProductSysNo
      ]]>
    </commandText>
    <parameters>
      <param name="@ProductSysNo" dbType="Int32" />
    </parameters>
  </dataCommand>

