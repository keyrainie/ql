  <dataCommand name="ECC_GetSOCSNote" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        SELECT TOP 1 ISNULL(NOTE, '') AS CSNote
        FROM IPP3.dbo.SO_Master WITH(NOLOCK)
        WHERE SysNo = @SOSysNo
      ]]>
    </commandText>
    <parameters>
      <param name="@SOSysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
  
