  <dataCommand name="CloseCommission" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        UPDATE OverseaPOASNManagement.dbo.Commission_Master
        SET [Status] = 'CLS'
        WHERE SysNo = @SysNo
			]]>
