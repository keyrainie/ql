<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation">
  <dataCommand name="GetEPortInfo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        SELECT c.[SysNo]
      ,[ePortName]
      ,[TaxFreeLimit]
      ,c.[Status]
      ,[PayType]
      ,[ShippingType]
      ,[Memo]
      ,[Indate]
      ,[InUser]
      ,[LastEditdate]
      ,[LastEditUser]
      ,u.[UserName] AS CreateUser
  FROM [IPP3].[dbo].[ePort] AS c
  INNER JOIN ipp3.dbo.Sys_User AS u
  ON c.InUser=u.SysNo
  WHERE c.SysNo=@SysNo
      ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
  <dataCommand name="CreateEPortInfo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        INSERT INTO [IPP3].[dbo].[ePort]
         (
            [ePortName]
            ,[TaxFreeLimit]
            ,[Status]
            ,[PayType]
            ,[ShippingType]
            ,[Memo]
            ,[Indate]
            ,[InUser]
            ,[LastEditdate]
            ,[LastEditUser]
         )
          VALUES
         (
                       @ePortName
            ,@TaxFreeLimit
            ,@Status
            ,@PayType
            ,@ShippingType
            ,@Memo
            ,@Indate
            ,@InUser
            ,@LastEditdate
            ,@LastEditUser
          )
          
         SELECT @SysNo = SCOPE_IDENTITY()
         
         SELECT 
            [SysNo]
            ,[ePortName]
            ,[TaxFreeLimit]
            ,[Status]
            ,[PayType]
            ,[ShippingType]
            ,[Memo]
            ,[Indate]
            ,[InUser]
            ,[LastEditdate]
            ,[LastEditUser]
             FROM
                  [IPP3].[dbo].[ePort] AS a WITH(NOLOCK)
             WHERE
                  a.SysNo=@SysNo
			]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" direction="Output"/>
      <param name="@ePortName" dbType="String" />
      <param name="@TaxFreeLimit" dbType="Int32" />
      <param name="@Status" dbType="Int32" />
      <param name="@PayType" dbType="String" />
      <param name="@ShippingType" dbType="Int32" />
      <param name="@Memo" dbType="String" />
      <param name="@Indate" dbType="DateTime" />
      <param name="@InUser" dbType="Int32" />
      <param name="@LastEditdate" dbType="DateTime" />
      <param name="@LastEditUser" dbType="Int32" />
    </parameters>
  </dataCommand>
  <dataCommand name="UpdateEPortInfo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
            UPDATE [IPP3].[dbo].[ePort]
            SET 
                  [ePortName] =ISNULL(@ePortName,ePortName)
            ,[TaxFreeLimit] = ISNULL(@TaxFreeLimit,TaxFreeLimit)
            ,[Status] = ISNULL(@Status,Status)
            ,[PayType] = ISNULL(@PayType,PayType)
            ,[ShippingType] = ISNULL(@ShippingType,ShippingType)
            ,[Memo] =ISNULL(@Memo,Memo)
            ,[Indate] =ISNULL(@Indate,Indate)
            ,[InUser] = ISNULL(@InUser,InUser)
            ,[LastEditdate] = ISNULL(@LastEditdate,LastEditdate)
            ,[LastEditUser] = ISNULL(@LastEditUser,LastEditUser)
             WHERE
                   SysNo=@SysNo
                   
             SELECT 
            [SysNo]
            ,[ePortName]
            ,[TaxFreeLimit]
            ,[Status]
            ,[PayType]
            ,[ShippingType]
            ,[Memo]
            ,[Indate]
            ,[InUser]
            ,[LastEditdate]
            ,[LastEditUser]
             FROM
                  [IPP3].[dbo].[ePort]
             WHERE
                  SysNo=@SysNo
			]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
      <param name="@ePortName" dbType="String" />
      <param name="@TaxFreeLimit" dbType="Int32" />
      <param name="@Status" dbType="Int32" />
      <param name="@PayType" dbType="String" />
      <param name="@ShippingType" dbType="Int32" />
      <param name="@Memo" dbType="String" />
      <param name="@Indate" dbType="DateTime" />
      <param name="@InUser" dbType="Int32" />
      <param name="@LastEditdate" dbType="DateTime" />
      <param name="@LastEditUser" dbType="Int32" />
    </parameters>
  </dataCommand>
  <dataCommand name="DeleteEPortInfo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
            UPDATE [IPP3].[dbo].[ePort]
            SET 
            [Status] = 0
             WHERE
                   SysNo=@SysNo
                   
             SELECT 
            [SysNo]
            ,[ePortName]
            ,[TaxFreeLimit]
            ,[Status]
            ,[PayType]
            ,[ShippingType]
            ,[Memo]
            ,[Indate]
            ,[InUser]
            ,[LastEditdate]
            ,[LastEditUser]
             FROM
                  [IPP3].[dbo].[ePort]
             WHERE
                  SysNo=@SysNo
			]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
  <dataCommand name="QeryEPortList" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
          SELECT @TotalCount = COUNT(c.SysNo) 
          FROM [IPP3].[dbo].[ePort] AS c WITH(NOLOCK) 
        #StrWhere#

        SELECT 
            [SysNo]
          ,[ePortName]
          ,[TaxFreeLimit]
          ,[Status]
          ,[PayType]
          ,[ShippingType]
          ,[Memo]
          ,[Indate]
          ,[InUser]
          ,[UserName] AS CreateUser
          ,[LastEditdate]
          ,[LastEditUser]
        FROM
        (
	        SELECT TOP (@EndNumber) 
            c.[SysNo]
          ,c.[ePortName]
          ,c.[TaxFreeLimit]
          ,c.[Status]
          ,c.[PayType]
          ,c.[ShippingType]
          ,c.[Memo]
          ,c.[Indate]
          ,c.[InUser]
          ,c.[LastEditdate]
          ,c.[LastEditUser]
          ,u.[UserName]
		        ,(ROW_NUMBER() OVER(ORDER BY #SortColumnName#)) AS RowNumber 
	        FROM [IPP3].[dbo].[ePort] AS c WITH(NOLOCK)
          INNER JOIN ipp3.dbo.Sys_User AS u
          ON c.InUser=u.SysNo
	        #StrWhere#
        ) Result 
        WHERE RowNumber > @StartNumber
      ]]>
    </commandText>
  </dataCommand>
  <dataCommand name="GetALLEPort" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        SELECT c.[SysNo]
      ,[ePortName]
      ,[TaxFreeLimit]
      ,c.[Status]
      ,[PayType]
      ,[ShippingType]
      ,[Memo]
      ,[Indate]
      ,[InUser]
      ,[LastEditdate]
      ,[LastEditUser]
      ,u.[UserName] AS CreateUser
  FROM [IPP3].[dbo].[ePort] AS c
  INNER JOIN ipp3.dbo.Sys_User AS u
  ON c.InUser=u.SysNo
      ]]>
    </commandText>
  </dataCommand>
</dataOperations>

