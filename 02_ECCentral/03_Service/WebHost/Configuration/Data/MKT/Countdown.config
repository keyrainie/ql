           ,@Status
           ,@AffectedStock        
           ,@Reasons
           ,@IsCountDownAreaShow
		       ,@PromotionType
		       ,@IsLimitedQty
		       ,@IsReservedQty
		       ,@CompanyCode		   
		       ,@IsPromotionSchedule
		       ,@PromotionTitle
		       ,@BaseLine
		       ,@MaxPerOrder		   
           ,@IsHomePageShow
           ,@IsTodaySpecials
           ,@Is24hNotice
           ,@IsShowPriceInNotice       
           ,@IsEndIfNoQty
           ,@AreaShowPriority
           ,@HomePagePriority
           ,@IsC1Show
           ,@IsC2Show
           ,@IsGroupOn
           ,@VendorSysNo
           ,@CreateUserName
		   )           
         SELECT @SysNo = SCOPE_IDENTITY()
                ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32"  direction="Output"/>
