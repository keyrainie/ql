           ,[Priority]
           ,[IconAddr]
           ,[Introduction]
           ,[IsRecommend]
           ,[GroupName]
           ,[MarketPrice]
           ,[GroupSysNo]
		       ,CompanyCode)
        VALUES
           (@SaleAdvSysNo
           ,@ProductSysNo
           ,@Priority
           ,@IconAddr
           ,@Introduction
           ,0
           ,@GroupName
           ,@MarketPrice
           ,@GroupSysNo
		       ,'8601')/*[Mark][Alan.X.Luo 硬编码]*/
