                           WHERE 
                               S.PromotionSysNo=G.SysNo 
                               AND BrandSysNo=@BrandSysNo 
                               AND Type='B' 
                               AND ComboType='N'
                        
                        )
     OR 
           Exists(
                           SELECT 
                               * 
                           FROM OverseaEcommerceManagement.dbo.Gift_SaleRules S WITH(NOLOCK) 
                           WHERE 
                               S.PromotionSysNo=G.SysNo 
                               AND C3SysNo=@C3SysNo 
                               AND Type='L' 
                               AND ComboType='N'
                        
                        )
      OR  
           Exists(
                           SELECT 
                               * 
                           FROM OverseaEcommerceManagement.dbo.Gift_SaleRules S WITH(NOLOCK) 
                           WHERE 
                               S.PromotionSysNo=G.SysNo 
                               AND C3SysNo=@C3SysNo 
                               AND BrandSysNo=@BrandSysNo 
                               AND Type='C' 
                               AND ComboType='N'
                   )
       )
       
      ]]>
