    <parameters>
      <param name="@StartDate" dbType="DateTime"/>
      <param name="@EndDate" dbType="DateTime"/>
      <param name="@BrandSysNo" dbType="Int32"/>
      <param name="@C3SysNo" dbType="Int32"/>
      <param name="@ProductSysNo" dbType="Int32"/>
    </parameters>
  </dataCommand>

