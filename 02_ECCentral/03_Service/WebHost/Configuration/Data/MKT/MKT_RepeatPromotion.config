FROM OverseaECommerceManagement.dbo.Gift G WITH(NOLOCK) 
WHERE 
    G.Status IN('R','A')
    AND IsGlobal!='Y' 
    AND Type='F'
    AND G.BeginDate < DATEADD(day,1,@EndDate)
    AND G.EndDate>= @StartDate 
    AND (
