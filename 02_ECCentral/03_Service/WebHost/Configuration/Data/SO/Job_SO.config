  <dataCommand name="SO_Update_SO4AuditUserInfo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        UPDATE IPP3.dbo.SO_Master 
        SET AuditUserSysNo = @AuditUserSysNo 
