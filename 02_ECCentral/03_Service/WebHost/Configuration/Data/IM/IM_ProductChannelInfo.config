  <dataCommand name="QueryProductChannelPeriodPrice" database="IMService" commandType="Text">
    <commandText>
      <![CDATA[
         SELECT 
    @TotalCount = COUNT(1) 
          FROM OverseaContentManagement.dbo.ChannelProductPeriodPrice WITH(NOLOCK)					
          #StrWhere#  ;
  
       WITH Result AS 
					(SELECT 
                 SysNo
                ,PeriodPrice
                ,BeginDate
                ,EndDate
                ,Status
                ,IsChangePrice
                ,Row_Number() OVER (ORDER BY #SortColumnName#) AS RowNumber 
          FROM OverseaContentManagement.dbo.ChannelProductPeriodPrice WITH(NOLOCK)
          #StrWhere#)                    

          SELECT 
                 SysNo
                ,PeriodPrice
                ,BeginDate
                ,EndDate
                ,Status
                ,IsChangePrice
          FROM RESULT 
          WHERE 
              RowNumber>@StartNumber 
              AND RowNumber<=@EndNumber 
          ORDER BY RowNumber
      ]]>
    </commandText>
    <parameters>
    </parameters>
  </dataCommand>

