    ,[ProductType] 
    ,[IsTakePictures] 
    ,[PackageList] 
    ,[ProductLink] 
    ,[Attention] 
    ,[Note] 
    ,[CompanyBelongs] 
    ,[OnlyForRank] 
    ,[PicNumber] 
    ,[HostWarrantyDay] 
    ,[PartWarrantyDay] 
    ,[Warranty] 
    ,[ServicePhone] 
    ,[ServiceInfo] 
    ,[IsOfferInvoice] 
    ,[Weight] 
    ,[IsLarge] 
    ,[Length] 
    ,[Width] 
    ,[Height] 
    ,[MinPackNumber] 
    ,[InDate] 
    ,[InUser] 
    ,[EditDate] 
    ,[EditUser] 
    ,[CompanyCode] 
    ,[StoreCompanyCode] 
    ,[LanguageCode] 
    ,[ProductDesc]
) 
VALUES
(
     @CommonSKUNumber 
    ,@ProductGroupSysno 
    ,@UPCCode 
    ,@BMCode
    ,@ProductName 
    ,@BriefName 
    ,@Keywords 
    ,@PMUserSysNo 
    ,@ProductModel 
    ,@ProductType 
    ,@IsTakePictures 
    ,@PackageList 
    ,@ProductLink 
