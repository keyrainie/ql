    ,@Weight 
    ,@IsLarge 
    ,@Length 
    ,@Width 
    ,@Height 
    ,@MinPackNumber 
    ,GETDATE() 
    ,@InUser 
    ,NULL 
    ,NULL 
    ,@CompanyCode 
    ,@StoreCompanyCode 
    ,@LanguageCode 
    ,@ProductDesc
)
SELECT @SysNo = SCOPE_IDENTITY();
	  ]]>
