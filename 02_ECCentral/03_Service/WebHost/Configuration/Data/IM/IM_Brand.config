  <dataCommand name="IsBrandInUsing" database="IMService" commandType="Text">
    <!--定义SQL语句-->
    <commandText>
      <![CDATA[      
      DECLARE @RESULT INT
      SELECT @RESULT =  COUNT(1) 
      FROM [IPP3].DBO.PRODUCT WITH(NOLOCK) 
      WHERE [STATUS] <> -1 AND BRANDSYSNO = @BrandSysNo

      SELECT @RESULT
	    ]]>
    </commandText>
