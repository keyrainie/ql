    <parameters>
      <param name="@ProductSysNo" dbType="Int32" />
      <param name="@OldVirtualPrice" dbType="Decimal"/>
      <param name="@NewVirtualPrice" dbType="Decimal"/>
    </parameters>
  </dataCommand>

