  <dataCommand name="InsertProductPriceRequest" database="IMService" commandType="Text">
    <commandText>
      <![CDATA[
		INSERT INTO IPP3.dbo.Product_Price_Request
		(
			ProductSysNo,
			BasicPrice,
			CurrentPrice,
			CashRebate,
			Point,
			Status,
			IsWholeSale,
			Q1,
			P1,
			Q2,
			P2,
			Q3,
			P3,
      Type,
      PMMemo,
      IsUseAlipayVipPrice,
      AlipayVipPrice,
      CreateUserSysNo,
      CreateTime
		)
		VALUES
		(
			@ProductSysNo,
			@BasicPrice,
			@CurrentPrice,
			@CashRebate,
			@Point,
			@Status,
			@IsWholeSale,
			@Q1,
			@P1,
			@Q2,
			@P2,
			@Q3,
			@P3,
