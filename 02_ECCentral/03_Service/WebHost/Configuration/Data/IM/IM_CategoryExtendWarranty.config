    <parameters>
      <param name="@BrandSysNo" dbType="Int32" />
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50" />
      <param name="@C3SysNo" dbType="Int32" />
      <param name="@InUser" dbType="String" />
