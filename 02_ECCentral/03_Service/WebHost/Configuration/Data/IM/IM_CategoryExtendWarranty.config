			    (
			    @BrandSysNo,
			    @C3SysNo,
			    @InUser,
			    GETDATE(),
			    @Status,
          @CompanyCode,
          @CompanyCode,
          @LanguageCode
			    )
					
			SET @SysNo = SCOPE_IDENTITY()
			]]>
