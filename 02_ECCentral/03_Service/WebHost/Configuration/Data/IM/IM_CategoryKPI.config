<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation">


  <dataCommand name="QueryCategoryKPIList" database="IMService" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT 
        @TotalCount = COUNT(1) 
      FROM [IPP3].[dbo].[Category3] A WITH (NOLOCK)
      Left join [IPP3].[dbo].[Category2] B WITH(NOLOCK)
      On B.SysNo=A.C2SysNo
      #StrWhere#
  
SELECT [SysNo]
      ,[CategorySysNo]
      ,[ParentSysNo]
      ,[CategoryID]
      ,[C3Name]
      ,[SearchByAttribute]
      ,[C3Bulletin]
      ,[Status]
      ,[MinMargin]
      ,[MinMarginPMD]
      ,[MarginModifyUserSysNo]
      ,[IsValuableProduct]
      ,[DMSRate]
      ,[IsMemberArea]
      ,[IsInstalmentProduct]
      ,[WarrantyDays]
      ,[RMARateStandard]
      ,[VirtualRate]
      ,[VirtualCount]
      ,[CheapenRisk]
      ,[OOSQty]
      ,[OOSRate]
      ,[PayPeriodType]
      ,[SafetyInventoryDay]
      ,[SafetyInventoryQty]
      ,[SafetyRevisedDay]
      ,[IsRequired]
      ,[IsFIFO]
      ,[MinCommission]
      ,[LargeFlag]
      ,[CompanyCode]
      ,[LanguageCode]
      ,[CurrencySysNo]
      ,[StoreCompanyCode]
      ,[Quota]
      ,Level
      FROM(
    SELECT  TOP (@EndNumber)
       A.[SysNo]
      ,A.SysNo AS [CategorySysNo]
      ,A.[C2SysNo] AS ParentSysNo
      ,A.[C3ID] AS CategoryID
      ,A.[C3Name]
      ,A.[SearchByAttribute]
      ,A.[C3Bulletin]
      ,A.[Status]
      ,A.[MinMargin]
      ,A.[MinMarginPMD]
      ,A.[MarginModifyUserSysNo]
      ,A.[IsValuableProduct]
      ,A.[DMSRate]
      ,A.[IsMemberArea]
      ,A.[IsInstalmentProduct]
      ,A.[WarrantyDays]
      ,A.[RMARateStandard]
      ,A.[VirtualRate]
      ,A.[VirtualCount]
      ,A.[CheapenRisk]
      ,A.[OOSQty]
      ,A.[OOSRate]
      ,A.[PayPeriodType]
      ,A.[SafetyInventoryDay]
      ,A.[SafetyInventoryQty]
      ,A.[SafetyRevisedDay]
      ,A.[IsRequired]
      ,A.[IsFIFO]
      ,A.[MinCommission]
      ,A.[LargeFlag]
      ,A.[CompanyCode]
      ,A.[LanguageCode]
      ,A.[CurrencySysNo]
      ,A.[StoreCompanyCode]
      ,A.[Quota]
      ,Level=3
      ,(ROW_NUMBER() OVER(ORDER BY #SortColumnName#)) AS RowNumber
    FROM [IPP3].[dbo].[Category3] A WITH(NOLOCK)
    Left join [IPP3].[dbo].[Category2] B WITH(NOLOCK)
    On B.SysNo=A.C2SysNo
    #StrWhere#) result
    WHERE RowNumber > @StartNumber
      ]]>
    </commandText>
    <parameters>
    </parameters>
  </dataCommand>

  <dataCommand name="QueryCategoryKPIList2" database="IMService" commandType="Text">
    <commandText>
      <![CDATA[
    SELECT 
    @TotalCount = COUNT(1) 
  FROM [IPP3].[dbo].[Category2_Property] A WITH (NOLOCK)
    RIGHT JOIN [IPP3].[dbo].[Category2] B WITH (NOLOCK)
    ON A.C2SysNo=B.Sysno AND A.CompanyCode=B.CompanyCode
#StrWhere#
  
SELECT [SysNo]
      ,[ParentSysNo]
      ,[CategoryID]
      ,[C2Name]
      ,[IsValuableProduct]
      ,[VirtualRate]
      ,[VirtualCount]
      ,[CheapenRisk]
      ,[OOSQty]
      ,[OOSRate]
      ,[Quota]
      ,[InDate]
      ,[InUser]
      ,[CompanyCode]
      ,[StoreCompanyCode]
      ,[LanguageCode]
      ,[EditDate]
      ,[EditUser]
      ,[MinMargin]
      ,[MinMarginPMD]
      ,[InStockDays]
      ,[MinCommission]
      ,Level
      FROM(
SELECT  TOP (@EndNumber)
      A.[IsValuableProduct]
      ,A.[VirtualRate]
      ,A.[VirtualCount]
      ,A.[CheapenRisk]
      ,A.[OOSQty]
      ,A.[OOSRate]
      ,A.[Quota]
      ,A.[InDate]
      ,A.[InUser]
      ,A.[CompanyCode]
      ,A.[StoreCompanyCode]
      ,A.[LanguageCode]
      ,A.[EditDate]
      ,A.[EditUser]
      ,B.[MinMargin]
      ,B.[MinMarginPMD]
      ,B.[InStockDays]
      ,B.[MinCommission]
      ,Level=2
      ,B.[SysNo] 
      ,B.C1SysNo AS ParentSysNo
      ,B.C2ID AS CategoryID
      ,B.C2Name
      ,B.Status
      ,(ROW_NUMBER() OVER(ORDER BY #SortColumnName#)) AS RowNumber
  FROM [IPP3].[dbo].[Category2_Property] A WITH(NOLOCK)
    RIGHT JOIN [IPP3].[dbo].[Category2] B WITH (NOLOCK)
    ON A.C2SysNo=B.Sysno 
  #StrWhere#) result
  WHERE RowNumber > @StartNumber
      ]]>
    </commandText>
    <parameters>
    </parameters>
  </dataCommand>
</dataOperations>