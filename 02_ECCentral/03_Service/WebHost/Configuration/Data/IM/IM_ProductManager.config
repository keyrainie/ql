    <parameters>
      <param name="@SysNo" dbType="Int32" />
      <param name="@PMUserSysNo" dbType="Int32" />
      <param name="@MaxAmtPerOrder" dbType="Decimal"/>
      <param name="@MaxAmtPerDay" dbType="Decimal" />
