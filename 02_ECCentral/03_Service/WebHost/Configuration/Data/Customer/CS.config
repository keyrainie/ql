  <dataCommand name="GetCSListBySysNo" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[			
SELECT [SysNo]
      ,[UserName]
      ,[IPPUserSysNo]
      ,[Role]
      ,[LeaderSysNo]
      ,[ManagerSysNo]
      ,[CreateDate]
      ,[CreateUserSysNo]
      ,[LastEditDate]
      ,[LastEditUserSysNo]
      ,[CompanyCode]
      ,[LanguageCode]
      ,[StoreCompanyCode]
      ,[Status]
      ,underlingnum = CASE [role]
                        WHEN 1
                        THEN 0
                        WHEN 3
                        THEN (SELECT Count(1)
                              FROM   [OverseaCustomerManagement].[dbo].[QCPlatForm_CSList]
                              WHERE  [ManagerSysNo] = a.SysNo)
                        ELSE (SELECT Count(1)
                              FROM   [OverseaCustomerManagement].[dbo].[QCPlatForm_CSList]
                              WHERE  [LeaderSysNo] = a.SysNo)
                      END
  FROM [OverseaCustomerManagement].[dbo].[QCPlatForm_CSList] as a WITH(NOLOCK)
  WHERE
  SysNo=@SysNo
  AND CompanyCode=@CompanyCode
                ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" />
    </parameters>
  </dataCommand>

