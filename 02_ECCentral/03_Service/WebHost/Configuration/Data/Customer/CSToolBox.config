    <parameters>
      <param name="@SysNo" dbType="Int32" />
      <param name="@Status" dbType="Int32" />
      <param name="@LastEditUserSysNo" dbType="Int32" property="[UserSysNo]"/>
    </parameters>
  </dataCommand>

