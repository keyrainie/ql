<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation"
				xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <dataCommand name="GetShipTypeSMSTemplateListByQuery" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[			
         SELECT @TotalCount = COUNT(SysNo) 
				  FROM OverseaCustomerManagement.[dbo].[SMSContentTemplate] WITH (NOLOCK)
				  #StrWhere#
				  
				 ;WITH TempTable AS ( 
						 SELECT [SysNo]
								 ,[Title]
								 ,[Template]
								 ,[CreateUserSysNo]
								 ,[CreateDate]
								 ,[LastEditUserSysNo]
								 ,[LastEditDate]
								 ,Row_Number() OVER (Order BY #SortColumnName#) AS RowNumber
					FROM OverseaCustomerManagement.[dbo].[SMSContentTemplate] WITH (NOLOCK)
					#StrWhere# )
					
				 SELECT [SysNo]
						 ,[Title]
						 ,[Template]
						 ,[CreateUserSysNo]
						 ,[CreateDate]
						 ,[LastEditUserSysNo]
						 ,[LastEditDate]
					FROM TempTable				
            WHERE RowNumber > @StartNumber AND RowNumber <= @EndNumber 					
      ]]>
    </commandText>
  </dataCommand>
</dataOperations>