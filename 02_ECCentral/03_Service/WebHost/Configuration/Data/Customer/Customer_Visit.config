      <![CDATA[			
				INSERT INTO [OverseaCustomerManagement].[dbo].[VisitMaintenanceLog]
           ([CustomerSysNo]
           ,[VisitSysNo]
           ,[SoSysNo]
           ,[CallStatus]
           ,[TelType]
           ,[RemindDate]
           ,[Note]
           ,[CreateDate]
           ,[CreateUserSysNo]
           ,[LastEditUserSysNo]
           ,[LastEditDate]
           ,[CompanyCode]
           )
     VALUES
           (@CustomerSysNo
           ,@VisitSysNo
           ,@SoSysNo
           ,@DealStatus
           ,@CallResult
           ,@RemindDate
           ,@Note
           ,@InDate
           ,@InUserSysNo
           ,@EditUserSysNo
           ,@EditDate
           ,@CompanyCode
           )
			  
		UPDATE [dbo].[ToVisitCustomer]
