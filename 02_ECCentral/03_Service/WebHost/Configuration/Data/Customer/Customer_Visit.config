  <dataCommand name="Customer_Visit_Query_WaittingVisit" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[	SELECT @TotalCount = COUNT(1)
FROM OverseaCustomerManagement.dbo.ToVisitCustomer vc WITH (NOLOCK) 
INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo		 
INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo	
#StrWhere#

;WITH Dbtemp AS (
	SELECT  Row_Number() OVER (Order BY #SortColumnName#) AS RowNumber
  ,vc.SysNo
  ,vc.LastCallTime
  ,e.LastBuyDate
	,c.SysNo CustomerSysNo
  ,c.CustomerID
  ,c.CustomerName
  ,c.Phone
  ,c.CellPhone
  ,c.Email
  ,c.[Rank]
  ,c.TotalSOMoney
  ,c.ConfirmedTotalAmt
  ,c.RegisterTime
	FROM OverseaCustomerManagement.dbo.ToVisitCustomer vc WITH (NOLOCK) 
	INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo		
  INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo	
	#StrWhere# )
SELECT   SysNo
        ,LastCallTime
        ,LastBuyDate
        ,CustomerSysNo
        ,CustomerID
        ,CustomerName
        ,Phone
        ,CellPhone
        ,Email
        ,[Rank]
        ,TotalSOMoney
        ,ConfirmedTotalAmt
        ,RegisterTime
FROM  Dbtemp  
WHERE RowNumber > @StartNumber  AND RowNumber <= @EndNumber 
ORDER BY rownumber 
      ]]>
    </commandText>
  </dataCommand>

  <dataCommand name="Customer_Visit_Query_FollowUpVisit" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[
        --查询已经回访过的客户，但是仍然需要跟进的客户
        SELECT @TotalCount = COUNT(VC.sysNo) 
        FROM OverseaCustomerManagement.dbo.VisitCustomer vc WITH (NOLOCK) 
        INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo	
        INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo
                #StrWhere# 
        
        
        ;WITH TBTemp AS ( SELECT ROW_NUMBER() OVER(order by #SortColumnName#) AS RowNumber
		        ,vc.SysNo
		        ,vc.LastCallStatus
		        ,vc.LastCallReason 
		        ,vc.SpiderAmt
		        ,vc.SpiderCount
		        ,vc.ContactCount
		        ,vc.LastCallTime
		        ,vc.LastEditUserSysno as EditUserSysNo
		        ,vc.NeedBuy
		        ,e.LastBuyDate
		        ,c.SysNo CustomerSysNo
		        ,c.CustomerID
		        ,c.CustomerName
		        ,c.Phone
		        ,c.CellPhone
		        ,c.Email
		        ,c.[Rank]
		        ,c.TotalSOMoney
		        ,c.ConfirmedTotalAmt
		        ,c.RegisterTime
	        FROM OverseaCustomerManagement.dbo.VisitCustomer vc WITH (NOLOCK) 
	        INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo	
	        INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo	
	        #StrWhere#  
                     )
          
        SELECT   SysNo
		        ,LastCallStatus
		        ,LastCallReason 
		        ,SpiderAmt
		        ,SpiderCount
		        ,ContactCount
		        ,LastCallTime
		        ,EditUserSysNo
		        ,NeedBuy
		        ,LastBuyDate
		        ,CustomerSysNo
		        ,CustomerID
		        ,CustomerName
		        ,Phone
		        ,CellPhone
		        ,Email
		        ,[Rank]
		        ,TotalSOMoney
		        ,ConfirmedTotalAmt
		        ,RegisterTime
        FROM TBTemp 
        WHERE RowNumber > @StartNumber  AND RowNumber <= @EndNumber 
        ORDER BY rownumber
				]]>
    </commandText>
  </dataCommand>

  <dataCommand name="Customer_Visit_Query_Visited" database="NCService" commandType="Text">
    <commandText>
      <![CDATA[ SELECT @TotalCount = COUNT(vc.SysNo)
	FROM OverseaCustomerManagement.dbo.VisitCustomer vc WITH (NOLOCK) 
	INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo	
	INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo	
	#StrWhere#

;WITH VO AS(
SELECT 
    CustomerSysNo 
   ,SUM([SOAmt])-SUM([RMAAmt]) AS VisitAmount 
   ,COUNT(1) AS OrderCount
FROM [OverseaCustomerManagement].[dbo].[VisitOrder] WITH(NOLOCK)
    #VisitOrderStrWhere#
GROUP BY CustomerSysNo 
)
,TBTemp AS 
(SELECT  Row_Number() OVER (Order BY #SortColumnName#) AS RowNumber
		,vc.SysNo
		,vc.LastMaintenanceStatus
		,vc.LastCallStatus
		,vc.LastCallReason 
		,vc.SpiderAmt
		,vc.SpiderCount
		,vc.ContactCount
		,vc.LastCallTime
		,vc.LastEditUserSysno as EditUserSysNo
		,vc.NeedBuy
		,vc.IsRMA
		,e.LastBuyDate
		,c.SysNo CustomerSysNo
		,c.CustomerID
		,c.CustomerName
		,c.Phone
		,c.CellPhone
		,c.Email
		,c.[Rank]
		,c.TotalSOMoney
		,c.ConfirmedTotalAmt
		,c.RegisterTime
		,ISNULL(VO.VisitAmount,0) AS VisitAmount
		,ISNULL(VO.OrderCount,0) AS OrderCount
	FROM OverseaCustomerManagement.dbo.VisitCustomer vc WITH (NOLOCK) 
	INNER JOIN IPP3.dbo.Customer c WITH(NOLOCK) ON c.SysNo = vc.CustomerSysNo	
	INNER JOIN IPP3.dbo.Customer_Extend e WITH(NOLOCK) ON c.SysNo = e.CustomerSysNo	    
	LEFT JOIN VO WITH(NOLOCK) ON VO.CustomerSysNo=vc.CustomerSysNo
#StrWhere#)

SELECT SysNo
		,LastMaintenanceStatus
		,LastCallStatus
		,LastCallReason 
		,SpiderAmt
		,SpiderCount
		,ContactCount
		,LastCallTime
		,EditUserSysNo
		,NeedBuy
		,IsRMA
		,LastBuyDate
		,CustomerSysNo
		,CustomerID
		,CustomerName
		,Phone
		,CellPhone
		,Email
		,[Rank]
		,TotalSOMoney
		,ConfirmedTotalAmt
		,RegisterTime
		,VisitAmount
		,OrderCount
FROM TBTemp 
WHERE RowNumber>@StartNumber AND RowNumber<=@EndNumber
				]]>
    </commandText>
  </dataCommand>

