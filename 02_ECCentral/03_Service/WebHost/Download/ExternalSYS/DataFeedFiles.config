<?xml version="1.0" encoding="utf-8" ?>
<DatafeedFiles  xmlns="http://IPP/web"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!--sourceType is mean "Item". System will insert this value to dbo.Datafeed_BatchUploadLog and dbo.Datafeed_DataItemMaster SourceType field-->
  <DatafeedFile name ="MKTPLS Item datafeed"
              sourceType = "I" 
              c3RowIndex="41"
                >
    <headers>
      <!--#Item-->
      <header name="#Item"  mappingPropertyName=""   Width="20" IsMustInput="false" Type="String" HeaderGroupIndex="0"/>
      <header name="分组标识"  mappingPropertyName="GroupIdentity"  IsReferenceCode="True" Width="4" IsMustInput="false" Type="String" HeaderGroupIndex="0"/>
      <header name="商家编号"  mappingPropertyName="SellerItemCode"  Width="4" IsMustInput="false" Type="String" HeaderGroupIndex="0"/>
      <header name="商品组标题" mappingPropertyName="ItemGroupTitle" Width="10" IsMustInput="false" Type="String" HeaderGroupIndex="0"/>
      <header name="生产商"  mappingPropertyName="Manufacturer" Width="10" IsMustInput="true" Type="List" HeaderGroupIndex="0"/>
      <header name="品牌" mappingPropertyName="Brand" Width="8" IsMustInput="true" Type="List" HeaderGroupIndex="0"/>
      <header name="型号系列" mappingPropertyName="Model" Width="8" IsMustInput="true" Type="String" HeaderGroupIndex="0"/>
      <!--供应商-->
      <header name="供应商" mappingPropertyName="" Width="10" IsMustInput="false" Type="String" HeaderGroupIndex="0"/>
      <!--PM-->
      <!--<header name="PM" mappingPropertyName="PM" Width="8" IsMustInput="true" Type="List" HeaderGroupIndex="0"/>-->
      <header name="商品名称" mappingPropertyName="ItemName" Width="8" IsMustInput="true" Type="String" HeaderGroupIndex="1"/>
      <!--<header name="商品简称" mappingPropertyName="ItemBrief" Width="5" IsMustInput="true" Type="String"/>-->
      <header name="具体型号" mappingPropertyName="CommonModel" Width="5" IsMustInput="true" Type="String" HeaderGroupIndex="1"/>
      <header name="商品详细描述" mappingPropertyName="ItemDescription" Width="15" IsMustInput="true" Type="String" HeaderGroupIndex="1"/>
      <header name="UPC" mappingPropertyName="UPCCode" Width="8" IsMustInput="false" Type=""  HeaderGroupIndex="1"/>
      <header name="厂商链接" mappingPropertyName="LinkUrl" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="1"/>
      <header name="搜索关键字" mappingPropertyName="SearchKey" Width="8" IsMustInput="false" Type="String"  HeaderGroupIndex="1"/>
      <!--<header name="产品图片URL" mappingPropertyName="" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="1"/>
      <header name="产品图片" mappingPropertyName="" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="1"/>-->
      <!--商品类型-->
      <!--<header name="商品类型" mappingPropertyName="" Width="10" IsMustInput="true" Type="List"/>-->
      <!--代销属性-->
      <header name="代销属性" mappingPropertyName="" Width="10" IsMustInput="true" Type="List" HeaderGroupIndex="1"/>
      <!--是否拍照-->
      <header name="是否拍照" mappingPropertyName="" Width="10" IsMustInput="true" Type="List" HeaderGroupIndex="1"/>
      
      <header name="泰隆优选供价(元)" mappingPropertyName="PurchasePrice" Width="6" IsMustInput="true" Type="Decimal"  HeaderGroupIndex="2"/>
      <header name="泰隆优选售价(元)" mappingPropertyName="RetailPrice" Width="6" IsMustInput="false" Type="Decimal"  HeaderGroupIndex="2"/>
      <!--毛利率-->
      <header name="毛利率" mappingPropertyName="" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="2"/>
      <header name="线下市场价(元)" mappingPropertyName="MKTPrice" Width="6" IsMustInput="false" Type="Decimal"  HeaderGroupIndex="2"/>
      <header name="京东售价(元)" mappingPropertyName="" Width="6" IsMustInput="false" Type="Decimal"  HeaderGroupIndex="2"/>
      <header name="卓越/易讯/一号店售价(元)" mappingPropertyName="" Width="12" IsMustInput="false" Type="Decimal"  HeaderGroupIndex="2"/>
      
      <header name="包装重量（单位：g）" mappingPropertyName="Weight" Width="8" IsMustInput="true" Type="Decimal"  HeaderGroupIndex="3"/>
      <header name="包装尺寸（长度，单位：mm）" mappingPropertyName="Length" Width="13" IsMustInput="true" Type="Decimal"  HeaderGroupIndex="3"/>
      <header name="包装尺寸（宽度，单位：mm）" mappingPropertyName="Width" Width="13" IsMustInput="true" Type="Decimal"  HeaderGroupIndex="3"/>
      <header name="包装尺寸（高度，单位：mm）" mappingPropertyName="Height" Width="13" IsMustInput="true" Type="Decimal"  HeaderGroupIndex="3"/>
      <header name="包装清单" mappingPropertyName="Package" Width="8" IsMustInput="false" Type="String"  HeaderGroupIndex="3"/>
      <header name="版本" mappingPropertyName="Version" Width="3" IsMustInput="false" Type="List"  HeaderGroupIndex="3"/>
      <header name="航空禁运" mappingPropertyName="Allowed" Width="5" IsMustInput="false" Type="List"  HeaderGroupIndex="3"/>
      <header name="最小包装数量" mappingPropertyName="MinPackageQty" Width="6" IsMustInput="false" Type="Int32"  HeaderGroupIndex="3"/>
      
      <header name="主件保修期(天)" mappingPropertyName="MasterWarranty" Width="7" IsMustInput="false" Type="Int32"  HeaderGroupIndex="4"/>
      <header name="附件保修期(天)" mappingPropertyName="SlaveWarranty" Width="7" IsMustInput="false" Type="Int32"  HeaderGroupIndex="4"/>
      <header name="保修细则" mappingPropertyName="WarrentyDetails" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="4"/>
      <!--ServicePhoneNumber-->
      <!--<header name="厂商售后电话" mappingPropertyName="" Width="10" IsMustInput="true" Type="String"  HeaderGroupIndex="0"/>-->
     
     
      <!--3C证书编号-->
      <header name="3C证书编号" mappingPropertyName="" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="5"/>
      <!--是否虚库-->
      <header name="是否虚库" mappingPropertyName="" Width="10" IsMustInput="false" Type="List"  HeaderGroupIndex="5"/>
      <header name="备注" mappingPropertyName="Memo" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="5"/>
      <!--附件资料地址-->
      <header name="附件资料地址" mappingPropertyName="" Width="10" IsMustInput="false" Type="String"  HeaderGroupIndex="5"/>
    </headers>
    <!--列分组信息-->
    <HeaderGroups>
      <HeaderGroup Index="0"  Name="基本信息" Color="47" />
      <HeaderGroup Index="1"  Name="商品描述" Color="57" />
      <HeaderGroup Index="2"  Name="商品价格" Color="51" />
      <HeaderGroup Index="3"  Name="包装信息" Color="50" />
      <HeaderGroup Index="4"  Name="保修" Color="46" />
      <HeaderGroup Index="5"  Name="其它" Color="31" />
      <HeaderGroup Index="6"  Name="特有属性" Color="40" />
    </HeaderGroups>
  </DatafeedFile>

</DatafeedFiles>
