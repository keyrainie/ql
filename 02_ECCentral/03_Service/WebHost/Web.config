      <add name="WebPrinter" path="WebPrinter.ashx" verb="*" type="ECCentral.Service.Utility.WebPrintHandler, ECCentral.Service.Utility" preCondition="integratedMode"/>
    </handlers>
    <directoryBrowse enabled="true" />
  </system.webServer>

