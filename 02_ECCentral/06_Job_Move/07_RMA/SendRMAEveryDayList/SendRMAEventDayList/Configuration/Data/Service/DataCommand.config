  <dataCommand name="RMARequestStatus21" database="OverseaServiceManagement" commandType="Text">
    <commandText>
      <![CDATA[  
			 SELECT   rma_register.sysno, 
         product.productid, 
         product.productname, 
         rma_register.customerdesc, 
         rma_register.status, 
         product.pmusersysno, 
