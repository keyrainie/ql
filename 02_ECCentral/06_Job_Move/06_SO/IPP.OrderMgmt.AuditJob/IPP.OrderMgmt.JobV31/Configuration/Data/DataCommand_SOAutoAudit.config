    <dataCommand name="GetSOList4Audit" database="OverseaOrderManagement" commandType="Text">
        <commandText>
            <![CDATA[
SELECT 
TOP #TOPCOUNT#
SM.[SysNo]
      ,SM.[SOID]
      ,SM.[Status]
      ,SM.[CustomerSysNo]
      ,SM.[StockSysNo]
      ,SM.[OrderDate]
      ,SM.[DeliveryDate]
      ,SM.[SalesManSysNo]
      ,SM.[IsWholeSale]
      ,SM.[IsPremium]
      ,SM.[PremiumAmt]
      ,SM.[ShipTypeSysNo]
      ,SM.[ShipPrice]
      ,SM.[PayTypeSysNo]
      ,SM.[PayPrice]
      ,SM.[SOAmt]
      ,SM.[DiscountAmt]
      ,SM.[PointAmt]
      ,SM.[CashPay]
      ,SM.[PointPay]
      ,SM.[ReceiveAreaSysNo]
      ,SM.[ReceiveContact]
      ,SM.[ReceiveName]
      ,SM.[ReceivePhone]
      ,SM.[ReceiveCellPhone]
      ,SM.[ReceiveAddress]
      ,SM.[ReceiveZip]
      ,SM.[AllocatedManSysNo]
      ,SM.[FreightUserSysNo]
      ,SM.[UpdateUserSysNo]
      ,SM.[UpdateTime]
      ,SM.[AuditUserSysNo]
      ,SM.[AuditTime]
      ,SM.[ManagerAuditUserSysNo]
      ,SM.[ManagerAuditTime]
      ,SM.[OutUserSysNo]
      ,SM.[OutTime]
      ,SM.[Memo]
      ,SM.[Note]
      ,SM.[InvoiceNote]
      ,SM.[IsVAT]
      ,SM.[IsPrintPackageCover]
      ,SM.[DeliveryMemo]
      ,SM.[PackageID]
      ,SM.[FinanceNote]
      ,SM.[InvoiceNo]
      ,SM.[PromotionCustomerSysNo]
      ,SM.[IsLarge]
      ,SM.[PositiveSOSysNo]
      ,SM.[IsMobilePhone]
      ,SM.[DeliveryTimeRange]
      ,SM.[PromotionCodeSysNo]
      ,SM.[HoldMark]
      ,SM.[HoldDate]
      ,SM.[HoldUser]
      ,SM.[HoldReason]
      ,SM.[ShoppingMasterSysNo]
      ,SM.[HaveAutoRMA]
      ,SM.[IsUseChequesPay]
      ,SM.[IsUsePrepay]
      ,SM.[PrepayAmt]
      ,SM.[GiftCardPay]
      ,SCKP.[SOSysNo]
      ,SCKP.[WeightSO]
      ,SCKP.[ShippingFee]
      ,SCKP.[PackageFee]
      ,SCKP.[RegisteredFee]
      ,SCKP.[Weight3PL]
      ,SCKP.[ShipCost]
      ,SCKP.[IsFPSO]
      ,SCKP.[FPReason]
      ,SCKP.[CustomerIPAddress]
      ,SCKP.[CustomerCookie]
      ,SCKP.[IsFPCheck]
      ,SCKP.[FPCheckTime]
      ,SCKP.[SpecialSOType]
      ,SCKP.[IsDuplicateOrder]
      ,SCKP.[ShipCost3PL]
	  ,SCKP.[SOType]
      ,SCKP.[IsDirectAlipay]
      ,SCKP.[MemoForCustomer]
      ,SCKP.[VPOStatus]
      ,SCKP.[IsPhoneOrder]
      ,SCKP.[OriginShipPrice]
      ,SCKP.[IsMultiInvoice]
      ,SCKP.[SplitUserSysNo]
      ,SCKP.[SplitDateTime]
      ,SCKP.[IsRequireShipInvoice]
      ,SCKP.[TenpayCoupon]
      ,SCKP.[MKTActivityType]
      ,SCKP.[IsVATPrinted]
      ,SCKP.[IsCombine]
      ,SCKP.[IsExtendWarrantyOrder]
      ,SCKP.[IsMergeComplete]
      ,SCKP.[MergeCompleteTime]
      ,SCKP.[IsBackOrder]
      ,SCKP.[DestWarehouseNumber]
      ,SCKP.[MergeOutTime]
      ,SCKP.[SoSplitType]
      ,SCKP.ShippingType
      ,SCKP.StockType
      ,SCKP.InvoiceType 
      ,SCKP.ReferenceSysno
      ,CSIN.Rank CustomerRank
      ,PC.PromotionValue
  FROM [IPP3].[dbo].[SO_Master] SM WITH(NOLOCK)
		LEFT JOIN [IPP3].[dbo].[SO_CheckShipping] SCKP WITH(NOLOCK)
			ON(SM.SysNo = SCKP.SOSysNo)
	    LEFT JOIN IPP3.dbo.Customer CSIN WITH(NOLOCK) 
            ON SM.CustomerSysno =CSIN.Sysno
      LEFT JOIN [IPP3].[dbo].[Promotion_Code] PC WITH(NOLOCK)
            ON SM.PromotionCodeSysNo = PC.SysNo
WHERE SM.Status = 0 
  AND ISNULL(SM.HoldMark,0) <> 1
  AND (SM.AuditUserSysNo IS NULL OR SM.AuditTime IS NULL)
  AND SCKP.IsFPCheck IS NOT NULL
  AND SCKP.[IsCombine] IS NOT NULL
  AND AuditType IS NULL
  AND SCKP.SOType <> 4
  AND SCKP.SOType <> 5
  AND SCKP.SOType <> 10
  AND ISNULL(SCKP.IsBackOrder,0)<>1 
  AND ISNULL(SCKP.SOType,0)<>1 
  AND ISNULL(SCKP.stockstatus,0)<>1
  AND ISNULL(SCKP.HoldStatus,0) =0
  AND (SCKP.SOType!=7 OR (SCKP.SOType=7 AND SCKP.SettlementStatus='S'))
  --AND ISNULL(SM.PromotionCodeSysNo,0) =0
  AND SM.CompanyCode=@CompanyCode
  --AND SM.PointPay = 0
  --AND ISNULL(SM.PrepayAmt,0) = 0
  --AND ISNULL(SM.GiftCardPay,0) = 0
        ]]>
        </commandText>
      <parameters>
        <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50"/>
      </parameters>
    </dataCommand>

