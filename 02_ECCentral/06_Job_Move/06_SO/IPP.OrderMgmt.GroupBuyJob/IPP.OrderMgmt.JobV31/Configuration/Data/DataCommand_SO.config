  <dataCommand name="GetSOMasterInfoBySysNo" database="OverseaOrderManagement" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT	
            SysNo 
           ,SOID 
           ,Status 
           ,CustomerSysNo 
           ,OrderDate 
           ,DeliveryDate 
           ,SalesManSysNo 
           ,IsWholeSale 
           ,IsPremium 
           ,PremiumAmt 
           ,ShipTypeSysNo 
           ,ShipPrice 
           ,PayTypeSysNo 
           ,PayPrice 
           ,SOAmt 
           ,DiscountAmt 
           ,PointAmt 
           ,CashPay 
           ,PointPay 
           ,ReceiveAreaSysNo 
           ,ReceiveContact 
           ,ReceiveName 
           ,ReceivePhone 
           ,ReceiveCellPhone 
           ,ReceiveAddress 
           ,ReceiveZip 
           ,AllocatedManSysNo 
           ,FreightUserSysNo 
           ,UpdateUserSysNo 
           ,UpdateTime 
           ,AuditUserSysNo 
           ,AuditTime 
           ,ManagerAuditUserSysNo 
           ,ManagerAuditTime 
           ,OutUserSysNo 
           ,OutTime 
           ,Memo 
           ,Note 
           ,InvoiceNote 
           ,IsVAT 
           ,IsPrintPackageCover 
           ,DeliveryMemo 
           ,FinanceNote 
           ,InvoiceNo 
           ,IsLarge 
           ,IsMobilePhone 
           ,DeliveryTimeRange 
           ,PromotionCodeSysNo 
           ,HoldMark 
           ,HoldDate 
           ,HoldUser 
           ,HoldReason 
           ,HaveAutoRMA 
           ,ShoppingMasterSysNo 
           ,IsUseChequesPay 
           ,IsUsePrepay 
           ,PrepayAmt 
           ,IsVIP 
           ,GiftCardPay
           ,ReceivingStatus 
        FROM	IPP3.dbo.v_so_master SM WITH(NOLOCK) 
					    WHERE	sysno = @SysNo
					    AND CompanyCode=@CompanyCode　
        ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32"/>
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50"/>
    </parameters>
  </dataCommand>

  <dataCommand name="GetSOItemInfoListBySOSysNo" database="OverseaOrderManagement" commandType="Text">
    <commandText>
      <![CDATA[				 
      SELECT	I.SysNo
                    ,I.SOSysNo
                    ,I.ProductSysNo
                    ,I.Quantity
                    ,I.Price
                    ,I.Cost
                    ,I.ProductType
                    ,I.BriefName
                    ,I.OriginalPrice
                    ,I.PromotionDiscount
                    ,I.MasterProductSysNo
                    ,W.SysNo As WarehouseNumber
                    ,W.StockName WarehouseName
                    ,p.Category3Sysno C3SysNo
                    ,CASE WHEN I.ProductType<>4 THEN P.ProductID ELSE '' END AS ProductID
                FROM	IPP3.dbo.V_SO_Item_ew I (nolock)
                LEFT JOIN OverseaContentManagement.dbo.V_CM_ItemBasicInfo   P  (nolock) on P.SysNo=I.ProductSysNo 
                LEFT JOIN OverseaInventoryManagement.dbo.V_INM_Stock W on W.SysNo = I.WarehouseNumber
                WHERE	I.SOSysNo=@SOSysNo
				AND I.CompanyCode=@CompanyCode
				
        ]]>
    </commandText>
    <parameters>
      <param name="@SOSysNo" dbType="Int32"/>
      <param name="@CompanyCode" dbType="AnsiStringFixedLength" size="50"/>
    </parameters>
  </dataCommand>

