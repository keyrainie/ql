                ([MailAddress]
                ,[MailSubject]
                ,[MailBody]
                ,Status
                ,Priority
                ,CompanyCode
                ,StoreCompanyCode
                ,LanguageCode
                )
				values
				(
				@MailAddress,
				@MailSubject,
				@MailBody,
				0,
				1,
        @CompanyCode,
        @StoreCompanyCode,
        @LanguageCode        
				)
			]]>
