<Publish>
  <Node>
    <RequestRoot>
      <MessageHeader>
        <Language>CH</Language>
        <CompanyCode>NewEgg</CompanyCode>
        <Version>0.1</Version>
        <MerchantId>来源商家</MerchantId>
        <Action>ImportAsn或UpdateAsn</Action>
      </MessageHeader>
      <Body>
        <Asn>
          <OrderCode>单据号码</OrderCode>
          <AsnType>入库类型</AsnType>
          <WarehouseCode>仓库编号</WarehouseCode>
          <Remark>入库备注</Remark>
          <Details>
            <Product Code="商品编码">商品数量</Product>
          </Details>
        </Asn>
      </Body>
    </RequestRoot>
  </Node>
</Publish>
