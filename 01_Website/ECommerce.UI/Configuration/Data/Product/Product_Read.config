  <!--获取商品运费 Key添加  2015-09-06-->
  <dataCommand name="Product_GetProductShippingPrice" database="Read" commandType="Text">
    <commandText>
      <![CDATA[
declare @MerchantSysNo int =(SELECT MerchantSysNo FROM IPP3.dbo.Product p WITH(NOLOCK) where SysNo=@ProductSysNo)

IF @MerchantSysNo=1
BEGIN
	SELECT
        p.SysNo as ProductSysNo
        ,ap.MerchantSysNo as [SellerSysNo]
        ,area.[StockSysNo]
        ,ap.[ShipTypeSysNo]
        ,st.ShipTypeName
		,ap.AreaSysNo
        ,ap.UnitPrice
        ,ap.BaseWeight
      FROM IPP3.dbo.Product p WITH(NOLOCK)
	    INNER JOIN OverseaContentManagement.dbo.ProductRestrictedArea area WITH(NOLOCK) 
		ON area.ProductSysNo = p.SysNo AND area.Status='A'
	    INNER JOIN [IPP3].[dbo].[ShipType_Area_Price] ap WITH(NOLOCK) 
		ON ap.AreaSysNo = area.CitySysNo AND ap.MerchantSysNo = p.MerchantSysNo
	    INNER JOIN [IPP3].[dbo].[ShipType] st WITH(NOLOCK) 
		ON st.SysNo=ap.ShipTypeSysNo
      WHERE p.SysNo = @ProductSysNo
      AND ap.AreaSysNo = @AreaSysNo
END
ELSE
BEGIN

	SELECT DISTINCT 
	 @ProductSysNo AS ProductSysNo
	,@MerchantSysNo AS [SellerSysNo]
	,area.[StockSysNo]
	,stap.[ShipTypeSysNo]
     ,st.[ShipTypeName]
     ,stap.AreaSysNo
     ,stap.UnitPrice
     ,stap.BaseWeight
    FROM [IPP3].[dbo].[ShipType_Area_Price] AS stap WITH(NOLOCK)
      INNER JOIN IPP3.dbo.ShipType AS st WITH(NOLOCK) ON stap.ShipTypeSysNo=st.SysNo
	  INNER JOIN IPP3.dbo.Area AS a WITH(NOLOCK) ON stap.AreaSysNo=a.SysNo
	 INNER JOIN OverseaContentManagement.dbo.ProductRestrictedArea area WITH(NOLOCK) 
		ON area.ProductSysNo = @ProductSysNo AND area.Status='A'
	 where stap.MerchantSysNo =@MerchantSysNo AND a.SysNo = @AreaSysNo 
		AND stap.ShipTypeSysNo IN (SELECT Shipping.SysNo
						FROM [ECStore].[dbo].[StockShipType](NOLOCK) StockShipping
						INNER JOIN IPP3.dbo.ShipType AS Shipping WITH(NOLOCK)
						ON StockShipping.ShipTypeSysNo = Shipping.SysNo 
						WHERE SellerSysNo = @MerchantSysNo and StockShipping.[Status]=0)
END

	


      ]]>
    </commandText>
    <parameters>
      <param name="@ProductSysNo" dbType="Int32" />
      <param name="@AreaSysNo" dbType="Int32" />
    </parameters>
  </dataCommand>
