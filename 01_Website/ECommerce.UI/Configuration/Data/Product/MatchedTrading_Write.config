<?xml version="1.0"?>
<dataOperations xmlns="http://oversea.ECommerce.com/DataOperation">
  <dataCommand name="CreateProductMatchedTrading" database="Write" commandType="Text">
    <commandText>
      <![CDATA[
          INSERT INTO [OverseaECommerceManagement].[dbo].[ProductMatchedTrading_Detail]
			   ([ProductSysNo]
			   ,[CustomerSysNo]
			   ,[Content]
			   ,[Status]
			   ,[Type]
			   ,[ReplyCount]
			   ,[InDate]
			   ,[InUser]
			   ,[EditDate]
			   ,[EditUser]
			   ,[CompanyCode]
			   ,[StoreCompanyCode]
			   ,[LanguageCode])
			 VALUES
			   (@ProductSysNo
			   ,@CustomerSysNo
			   ,@Content
			   ,@Status
			   ,@Type
			   ,0
			   ,getdate()
			   ,'WebSite'
			   ,getdate()
			   ,''
			   ,@CompanyCode
			   ,@StoreCompanyCode
			   ,@LanguageCode)
          ]]>
    </commandText>
    <parameters>
      <param name="@ProductSysNo" dbType="Int32" />
      <param name="@CustomerSysNo" dbType="Int32" />
      <param name="@Content" dbType="String" size="2000" />
      <param name="@Type" dbType="Int32"/>
      <param name="@LanguageCode" dbType="String" />
      <param name="@CompanyCode" dbType="String" />
      <param name="@StoreCompanyCode" dbType="String" />
      <param name="@Status" dbType="String" />
    </parameters>
  </dataCommand>

  <dataCommand name="GetProductMatchedTrading" database="Read" commandType="Text">
    <commandText>
      <![CDATA[
       SELECT 
         [SysNo]
         ,[ProductSysNo]
			   ,[CustomerSysNo]
			   ,[Content]
			   ,[Status]
			   ,[Type]
			   ,[ReplyCount]
			   ,[InDate]
			   ,[InUser]
			   ,[EditDate]
			   ,[EditUser]
			   ,[CompanyCode]
			   ,[StoreCompanyCode]
			   ,[LanguageCode]
			FROM 
       [OverseaECommerceManagement].[dbo].[ProductMatchedTrading_Detail] WITH(NOLOCK)
       WHERE Sysno=@SysNo
          ]]>
    </commandText>
    <parameters>
      <param name="@SysNo" dbType="Int32" />
    </parameters>
  </dataCommand>

  <dataCommand name="CreateProductMatchedTradingReply" database="Write" commandType="Text">
    <commandText>
      <![CDATA[
         INSERT INTO [OverseaECommerceManagement].[dbo].[ProductMatchedTrading_Reply]
           ([ConsultSysNo]
           ,[CustomerSysNo]
           ,[Content]
           ,[Status]
           ,[Type]
           ,[IsTop]
           ,[InDate]
           ,[InUser]
           ,[EditDate]
           ,[EditUser]
           ,[CompanyCode]
           ,[StoreCompanyCode]
           ,[LanguageCode]
           ,[NeedAdditionalText])
		VALUES
           (@ConsultSysNo
           ,@CustomerSysNo
           ,@Content
           ,'O'
           ,'W'
           ,'N'
           ,getdate()
           ,'WebSite'
           ,getdate()
           ,''
           ,@CompanyCode
           ,@StoreCompanyCode
           ,@LanguageCode
           ,'N')
          ]]>
    </commandText>
    <parameters>
      <param name="@ConsultSysNo" dbType="Int32" />
      <param name="@CustomerSysNo" dbType="Int32" />
      <param name="@Content" dbType="String" size="2000" />
      <param name="@LanguageCode" dbType="String" />
      <param name="@CompanyCode" dbType="String" />
      <param name="@StoreCompanyCode" dbType="String" />
    </parameters>
  </dataCommand>
</dataOperations>

