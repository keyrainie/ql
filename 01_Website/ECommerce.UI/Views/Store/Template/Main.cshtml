@using ECommerce.Entity.Store
@model StorePageTemplate
@{
    var main = Model.StorePageLayouts.Find(p => p.PageLayoutKey.Trim().Equals("Main", StringComparison.CurrentCultureIgnoreCase));

}
<div class="store_layout_main">
    <div class="wraper cls">
        <div class="store_layout_col_main">


            <!--Main-->
            @if (main != null && main.StorePageElements != null)
            {
                main.StorePageElements.ForEach(p =>
                    {
                        Html.RenderPartial(p.FrontPartialViewPath, p);
                    });
            }
        </div>
    </div>

</div>
