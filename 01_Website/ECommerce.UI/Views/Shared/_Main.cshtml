    @BuildJsRef("/Resources/scripts/common/common.js")
    @BuildJsRef("/Resources/scripts/common/MiniAccount.js")
    @BuildJsRef("/Resources/scripts/pages/shopping/ShoppingCartMini.js")
    @BuildJsRef("/Resources/scripts/pages/home/_main.js")
    @BuildCssRef("/Resources/themes/default/css/global.css")
    @BuildCssRef("/Resources/themes/default/css/mini_login.css")
    @RenderSection("rsHeader", required: false)



    @BuildCssRef("/Resources/themes/default/css/bootstrap.css")
    @BuildCssRef("/Resources/themes/default/css/font-awesome.css")
    @BuildCssRef("/Resources/themes/default/css/style.css")
     @BuildCssRef("/Resources/themes/default/css/Index.css")
