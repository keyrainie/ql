    <meta name="description" content="@(string.IsNullOrWhiteSpace(ViewBag.SEODescription) ? "全球购物" : ViewBag.SEODescription),全球购物官方网站" />
    @if (!string.IsNullOrWhiteSpace(ViewBag.SEOPageAdditionContent))
    {
        @Html.Raw(ViewBag.SEOPageAdditionContent)
