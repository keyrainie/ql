@{
    ViewBag.Title = "支付完成";
    Layout = "~/Views/Shared/_Master.cshtml";
}

@section Section_Header{
    @BuildCssRef("/Resources/style/checkout.css")
}
@section Section_Content{
    <div id="main">
         <div class="checkout_order_succ">
            <article class="order_finish_titgroup">
                <h2 class="msg">您的订单已支付完成！</h2>
                <div class="action">
                    <a class="btn_shopping" href="@BuildUrl("CategoryAll")">继续购物</a>
                    <a class="btn_gray" href="@(BuildUrl("Order_List"))">查看订单</a>
                </div>
            </article>
        </div>
    </div>
    <script type="text/javascript">
        setTimeout(function () {
            window.location.href = '@(BuildUrl("Order_List"))';
        }, 5000);
    </script>
}